INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CSC', '493', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
