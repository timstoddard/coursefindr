INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('SPAN', '233', 'Introduction to Hispanic Readings', 'Lec', '1', 'Selected readings from Hispanic authors that show the Hispanic literary tradition from the Middle Ages to the present in Spain, Latin America, and of Latino/a writers in the United States.  May include film and other media.  Conducted in Spanish.  4 lectures.  Fulfills GE C1.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('SPAN', '301', 'Advanced Composition in Spanish', 'Lec', '1', 'Oral and written development of structural grammar, syntax, and complex components of Spanish.  Vocabulary expansion and idiomatic construction.  Written compositions.  May include Spanish/English translation.  Conducted in Spanish.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('SPAN', '305', 'Significant Works in Spanish', 'Lec', '1', 'Critical analysis and oral discussion of poetry, essays, novels, plays, films, and other media by selected Hispanic authors.  Conducted in Spanish.  The Schedule of Classes will list topic selected.  Total credit limited to 12 units.  4 lectures.  Fulfills GE C4 except for Modern Languages and Literatures majors.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('SPAN', '340', 'Chicano/a Authors', 'Lec', '1', 'Introduction to Chicano/a literary accomplishments to facilitate appreciation of Chicano/a literary aesthetics and increase understanding of Chicano/a cultural values and lifestyles.  Conducted in Spanish.  4 lectures.  Fulfills GE C4 except for Modern Languages and Literatures majors.  Fulfills USCP.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('SPAN', '402', 'Advanced Linguistics in Spanish', 'Lec', '1', 'Aspects of Spanish linguistics today.  Topics may include morphology, semantics, syntax, phonetics, phonology, theoretical linguistics, history of the language, and teaching methodology and applied linguistics in Spanish.  Conducted in Spanish.  The Schedule of Classes will list topic selected.  Total credit limited to 8 units.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('SPAN', 'P102', 'Elementary Spanish II', 'Lec', '1', 'Class practice and assigned outside work in pronunciation, sentence structure, reading, writing, and conversation.  Language taught in its intercultural context.  Not open to students with credit in SPAN 104.  To be taken in numerical sequence.  3 lectures, 1 activity.', '4');
