INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DATA', '301', 'Introduction to Data Science', 'Lec', '2', 'Introduction to the field of data science and the workflow of a data scientist.  Types of data (tabular, textual, sparse, structured, temporal, geospatial), basic data management and manipulation, simple summaries, and visualization.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DATA', '301', 'Introduction to Data Science', 'Lab', '2', 'Introduction to the field of data science and the workflow of a data scientist.  Types of data (tabular, textual, sparse, structured, temporal, geospatial), basic data management and manipulation, simple summaries, and visualization.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '100', 'Enterprise Project', 'Ind', '2', 'Post-harvest processing of a high quality food product.  Project participation is voluntary and subject to approval by the department head and the Cal Poly Corporation.  Total degree credit for FSN 201 and FSN 401 combined limited to 12 units.  Credit/No Credit grading only.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '200', 'Special Problems for Undergraduates', 'Ind', '3', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 4 units, with a maximum of 2 units per quarter.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '230', 'General Dairy Husbandry', 'Lec', '1', 'Selection, breeding, feeding, and management of dairy cattle. Composition and food value of dairy products. Milk pricing, political influences, dairy industry statistics and opportunities. Producing and handling products. Intended as introductory course for non-dairy science majors. 3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '230', 'General Dairy Husbandry', 'Lab', '3', 'Selection, breeding, feeding, and management of dairy cattle. Composition and food value of dairy products. Milk pricing, political influences, dairy industry statistics and opportunities. Producing and handling products. Intended as introductory course for non-dairy science majors. 3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '233', 'Milk Processing and Inspection', 'Lec', '1', 'Composition and properties of fluid milk and its constituents.  Equipment used to handle, process, and distribute fluid milk and related products.  California dairy codes used for dairy farms and plants, with practice inspections of dairy farms and factories.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '233', 'Milk Processing and Inspection', 'Lab', '2', 'Composition and properties of fluid milk and its constituents.  Equipment used to handle, process, and distribute fluid milk and related products.  California dairy codes used for dairy farms and plants, with practice inspections of dairy farms and factories.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '270', 'Selected Topics', 'Lec', '1', 'Directed group study of selected topics.  The Schedule of Classes will list title selected.  Total credit limited to 8 units.  1 to 4 lectures.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '333', 'Dairy Animal Health, Safety and Applied Technology', 'Lec', '1', 'Application of principles of herd health, biosecurity, lactation physiology, cattle management and reproductive physiology to successful dairy operations.  Assessment of animal comfort and general healthy and well-being.  Practical techniques in safe animal handling.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '333', 'Dairy Animal Health, Safety and Applied Technology', 'Act', '2', 'Application of principles of herd health, biosecurity, lactation physiology, cattle management and reproductive physiology to successful dairy operations.  Assessment of animal comfort and general healthy and well-being.  Practical techniques in safe animal handling.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '339', 'Internship in Dairy Science', 'Ind', '2', 'Selected Dairy Science students will spend up to 12 weeks with an approved agricultural firm engaged in production or related business.  Time will be spent applying and developing production and managerial skills and abilities.  One unit of credit may be allowed for each full week of completed and reported internship.  Degree credit limited to 12 units.  Credit/No Credit grading only.', '1-12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '4', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '412', 'Dairy Farm Consultation', 'Sem', '1', 'Student consultation teams of three or four students visit dairies and/or attend management training seminars followed by presenting management recommendations to the dairy owners, consultants, and other industry leaders.  1 seminar and supervised work.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '412', 'Dairy Farm Consultation', 'Ind', '1', 'Student consultation teams of three or four students visit dairies and/or attend management training seminars followed by presenting management recommendations to the dairy owners, consultants, and other industry leaders.  1 seminar and supervised work.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '432', 'Advanced Dairy Herd Management', 'Lec', '1', 'Dairy herd management skills needed in dairy operations.  Instruction and lab experience in management, records, labor, waste management, and milking management.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '461', 'Senior Project', 'Lec', '1', 'Completion, under faculty supervision, of an investigative project typical of problems which graduates must solve in their fields of employment.  Required minimum of 90 hours.  Analytical, formal report is required.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '461', 'Senior Project', 'Ind', '4', 'Completion, under faculty supervision, of an investigative project typical of problems which graduates must solve in their fields of employment.  Required minimum of 90 hours.  Analytical, formal report is required.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '500', 'Individual Study in Dairy Science', 'Ind', '2', 'Advanced independent study planned and completed under the direction of a member of the Dairy Science faculty.  Total credit limited to 6 units.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '539', 'Graduate Internship in Dairy Science', 'Ind', '2', 'Application of theory to the solution of problems of agricultural production or related business in the field of Dairy Science.  Analyze specific management problems and perform general management assignments detailed in a contract between the student, the firm or organization, and the faculty advisor before the internship commences.  Degree credit limited to 6 units.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '581', 'Graduate Seminar in Dairy Science', 'Sem', '1', 'Current findings and research problems in the field and their application to industry.  Group study of current problems of industry.  Current experimental and research findings as applied to production and marketing.  Repeatable for up to 3 units.  Credit/No Credit grading only.  1-3 seminars.  Credit/ no credit grading only.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '585', 'Cooperative Education Experience in Dairy Science', 'Ind', '1', 'Advanced study, analysis and part-time work experience in the field; current innovations, practices, and problems in administration, supervision, and organization of business, industry, and government.  Must have demonstrated ability to do independent work and research in career field.  Total credit limited to 9 units.  Credit/No Credit grading only.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', '599', 'Thesis in Dairy Science', 'Ind', '2', 'Systematic research of a significant problem in Dairy Science.  Thesis will include problem identification, significance, methods, data analysis, and conclusion.  Students must enroll every quarter in which facilities are used or advisement is received.  Degree credit limited to 6 units.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', 'P233', 'Milk Processing and Inspection', 'Lec', '1', 'Composition and properties of fluid milk and its constituents.  Equipment used to handle, process, and distribute fluid milk and related products.  California dairy codes used for dairy farms and plants, with practice inspections of dairy farms and factories.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', 'P233', 'Milk Processing and Inspection', 'Lab', '1', 'Composition and properties of fluid milk and its constituents.  Equipment used to handle, process, and distribute fluid milk and related products.  California dairy codes used for dairy farms and plants, with practice inspections of dairy farms and factories.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', 'S502', 'Dairy Chemistry Laboratory', 'Lab', '1', 'Objective measurements, analysis and isolation of milk components.  Experimental demonstration of chemical and physical reactions of milk components during typical processing conditions.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', 'S521', 'Dairy Processing and Manufacturing II', 'Lec', '1', 'Unit operations in process systems for the efficient manufacture of fluid milk products, cream, butter, and concentrated milk.  Process variables and their control, system integration.  Use of processes to manipulate physical and chemical properties to influence product quality.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', 'S521', 'Dairy Processing and Manufacturing II', 'Lab', '1', 'Unit operations in process systems for the efficient manufacture of fluid milk products, cream, butter, and concentrated milk.  Process variables and their control, system integration.  Use of processes to manipulate physical and chemical properties to influence product quality.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', 'S541', 'Quality Assurance, Quality Control and Food Safety', 'Lec', '1', 'Concepts and methodologies used in dairy industry and dairy food plants for assurance and control of the quality of finished product.  Basis for understanding physical, chemical and microbiological methods as they apply to quality evaluation of dairy foods.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', 'S541', 'Quality Assurance, Quality Control and Food Safety', 'Lab', '1', 'Concepts and methodologies used in dairy industry and dairy food plants for assurance and control of the quality of finished product.  Basis for understanding physical, chemical and microbiological methods as they apply to quality evaluation of dairy foods.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', 'S581', 'Graduate Seminar in Dairy Science', 'Sem', '1', 'Current findings and research problems in the field and their application to industry.  Group study of current problems of industry.  Current experimental and research findings as applied to production and marketing.  Repeatable for up to 3 units.  Credit/No Credit grading only.  1-3 seminars.  Credit/ no credit grading only.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DSCI', 'S582', 'Dairy Processing and Plant Management', 'Lab', '1', 'Dairy plant operation, processing experience and production team dynamics.  Practical training and experience in dairy plant operations to allow efficient, safe and sanitary processing of dairy products manufacture in a team environment.  Total credit limited to 4 units.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '201', 'Survey of Economics', 'Lec', '3', 'Basic principles of microeconomics and macroeconomics. Emphasis on applications to current national and global economic issues. For majors requiring one quarter of economics. Not open to students having previous credit in ECON 222 or equivalent. 4 lectures. Crosslisted as ECON/HNRS 201. Fulfills GE D2.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '221', 'Microeconomics', 'Lec', '3', 'Microeconomic principles. Marginal and equilibrium analysis of commodity and factor markets in determination of price and output. Normative issues of efficiency and equity. 4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '222', 'Macroeconomics', 'Lec', '6', 'Introduction to economic problems. Macroeconomic analysis and principles. Aggregate output, employment, prices, and economic policies for changing these variables. International trade and finance. Issues of economic growth and development. Comparative economic systems and economies in transition. 4 lectures. Fulfills GE D2.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '303', 'Economics of Poverty, Discrimination and Immigration', 'Lec', '2', 'Economic analysis of the cause, extent and impact of poverty, discrimination and immigration and of the policies designed to address these socioeconomic issues.  Emphasis on the experience of African-Americans, Latinos, and women in the United States.  4 lectures.  Crosslisted as ECON/HNRS 303.  Fulfills GE D5 except for Economics majors.  Fulfills USCP.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '304', 'Comparative Economic Systems', 'Lec', '1', 'Analysis of economic systems as a set of mechanisms and institutions for decision making, and the implementation of decisions regarding income distribution, the levels of consumption and production, and the level of economic welfare.  4 lectures.  Fulfills GE D5 except for Economics majors.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '311', 'Intermediate Microeconomics I', 'Lec', '1', 'Consumer behavior and the theory of demand; production, cost, supply functions; perfect competition; monopoly and oligopoly; labor markets.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '312', 'Intermediate Microeconomics II', 'Lec', '1', 'Game theory; risk, uncertainty and information; choice over time; asset markets; general equilibrium; welfare economics, externalities and public goods.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '313', 'Intermediate Macroeconomics', 'Lec', '1', 'Analysis of national income, price level, employment, international trade and economic growth.  Development of the theory of national income determination.  Evaluation of roles of monetary and fiscal policy.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '330', 'International Trade Theory', 'Lec', '1', 'Theory of comparative advantage, neoclassical model of trade, offer curves and terms of trade, edgeworth boxes, valuation of factor inputs, effects of migration and mobility of funds, emerging growth and trade distortions, welfare effects of trade, and recent developments in trade theory.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '339', 'Econometrics', 'Lec', '1', 'Application of statistical methods useful in economics.  General linear regression model.  Specific issues and problems related to economic models:  multicollinearity, autocorrelation, heteroscedasticity, dummy variables, lagged variables, and simultaneous equation estimation.  Application and evaluation of selected examples of empirical economic research.  Microcomputer applications.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '339', 'Econometrics', 'Act', '1', 'Application of statistical methods useful in economics.  General linear regression model.  Specific issues and problems related to economic models:  multicollinearity, autocorrelation, heteroscedasticity, dummy variables, lagged variables, and simultaneous equation estimation.  Application and evaluation of selected examples of empirical economic research.  Microcomputer applications.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '400', 'Special Problems', 'Ind', '1', 'Supervised investigation, including a written report, of a topic chosen with prior approval of instructor.  Total credit limited to 4 units.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '403', 'Industrial Organization', 'Lec', '1', 'Application of basic tools of economics to American Industry.  Case studies of individual firms and industries.  Performance of various business structures, such as monopoly and oligopoly.  Effects of government regulation and antitrust policy.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '404', 'International Trade Theory', 'Lec', '1', 'Theory of comparative advantage, neoclassical model of trade, offer curves and terms of trade, edgeworth boxes, valuation of factor inputs, effects of migration and mobility of funds, emerging growth and trade distortions, welfare effects of trade, and recent developments in trade theory.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '408', 'Mathematical Economics', 'Lec', '1', 'Applications of quantitative techniques to topics in microeconomic and macroeconomic theory.  Use of multivariate calculus and linear algebra in formulating static economic models.  Applications of statistical inference, estimation and forecasting in economic models.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '409', 'Probability Models for Economic Decisions', 'Lec', '1', 'Decision making in complex, realistic situations.  Simulation of random variables in Excel.  Risk aversion.  Subjective assessment of probabilities and correlations.  Decision trees.  Optimal bidding in auctions.  The winners curse.  Moral hazard and risk sharing.  Repeated investment decisions under risk.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '461', 'Senior Project I', 'Ind', '1', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ECON', '462', 'Senior Project II', 'Ind', '1', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDES', '123', 'Principles of Environmental Design', 'Lec', '1', 'Introduction to the individuals and societal relation with the designed and built environment, and its impact on natural resource consumption, identity, behavior, community, and human health, safety, and general well-being.  Study of the individual and role of design in community development and in making and preserving culture.  Diverse perspectives including designers, engineers, governing bodies, and individual users.  2 lectures, 2 discussions.  Fulfills GE D4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDES', '123', 'Principles of Environmental Design', 'Dis', '8', 'Introduction to the individuals and societal relation with the designed and built environment, and its impact on natural resource consumption, identity, behavior, community, and human health, safety, and general well-being.  Study of the individual and role of design in community development and in making and preserving culture.  Diverse perspectives including designers, engineers, governing bodies, and individual users.  2 lectures, 2 discussions.  Fulfills GE D4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDES', '408', 'Implementing Sustainable Principles', 'Lec', '1', 'A primarily project-based course, intended to aid students who wish to collaborate with the purpose of implementing sustainability principles by developing tools, process or designs, for community-based projects and proposals at various scales of planning, architecture and design of the human environment to address social, environmental and economic issues.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDES', 'P123', 'Principles of Environmental Design', 'Lec', '1', 'Introduction to the individuals and societal relation with the designed and built environment, and its impact on natural resource consumption, identity, behavior, community, and human health, safety, and general well-being.  Study of the individual and role of design in community development and in making and preserving culture.  Diverse perspectives including designers, engineers, governing bodies, and individual users.  2 lectures, 2 discussions.  Fulfills GE D4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDES', 'P123', 'Principles of Environmental Design', 'Dis', '1', 'Introduction to the individuals and societal relation with the designed and built environment, and its impact on natural resource consumption, identity, behavior, community, and human health, safety, and general well-being.  Study of the individual and role of design in community development and in making and preserving culture.  Diverse perspectives including designers, engineers, governing bodies, and individual users.  2 lectures, 2 discussions.  Fulfills GE D4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '207', 'Diverse Student Learning and Development in Educational Settings', 'Lec', '2', 'Application of theories and research on the development and learning of children and young adolescents within diverse backgrounds and in relation to the teaching-learning process.  Observations/interactions with children in school settings.  3 lectures, 1 activity.  Participation in public schools requires mandated fingerprint clearance.  Crosslisted as CD/EDUC 207.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '207', 'Diverse Student Learning and Development in Educational Settings', 'Act', '2', 'Application of theories and research on the development and learning of children and young adolescents within diverse backgrounds and in relation to the teaching-learning process.  Observations/interactions with children in school settings.  3 lectures, 1 activity.  Participation in public schools requires mandated fingerprint clearance.  Crosslisted as CD/EDUC 207.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '300', 'Historical, Philosophical, and Social Foundations of Public Education', 'Lec', '2', 'Historical, philosophical, and social foundations of public education in relation to school curriculum, instruction and dispositions of effective teachers.  Structured observation and participation in K-12 public schools with attention to instructional practices for diverse learners.  2 lectures, 1 activity.  Participation in public schools requires mandated fingerprint clearance.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '300', 'Historical, Philosophical, and Social Foundations of Public Education', 'Act', '2', 'Historical, philosophical, and social foundations of public education in relation to school curriculum, instruction and dispositions of effective teachers.  Structured observation and participation in K-12 public schools with attention to instructional practices for diverse learners.  2 lectures, 1 activity.  Participation in public schools requires mandated fingerprint clearance.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '400', 'Special Problems for Undergraduates', 'Ind', '3', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 4 units, with a maximum of 2 units per quarter.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '410', 'Social, Historical and Ethical Perspectives on Teaching and Learning', 'Lec', '1', 'Inquiry into the social, historical, philosophical and psychological foundations of education with an emphasis on applying educational theory to practice.  Prepares Single Subject Credential Program students for classroom teaching.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '416', 'Literacy, Language, and Culture in Content Area Classrooms', 'Lec', '2', 'Theories and application of literacy learning, assessment and second language acquisition in content classrooms.  Observation of classrooms, tutoring English language learners, designing and teaching literacy lessons, planning and implementing assessments across content areas.  Recognition of the role of culture in language acquisition.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '417', 'Literacy, Language, and Culture in the Content Area Classroom Fieldwork', 'Act', '1', 'Fieldwork in the secondary classroom related to literacy, language and culture for Agriculture Education candidates.  Includes 1-3 hours of observation each week.  Participation in public schools requires mandated fingerprint clearance.  Credit/No Credit grading.  1 activity.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '423', 'Bilingual Literacy', 'Sem', '1', 'Patterns of classroom organization, application of reading programs, approaches, methods in English and Spanish, and supervised field experiences in elementary classrooms with bilingual students.  3 seminars, 1 activity.  Limited to students seeking BCLAD certification.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '423', 'Bilingual Literacy', 'Act', '1', 'Patterns of classroom organization, application of reading programs, approaches, methods in English and Spanish, and supervised field experiences in elementary classrooms with bilingual students.  3 seminars, 1 activity.  Limited to students seeking BCLAD certification.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '427', 'Theories, Methods, and Assessment of First and Second Language Acquisition in Schools', 'Sem', '2', 'Theories, methods, materials and assessment involved in the instruction of limited English proficient (L.E.P.) students.  Bilingual, transitional, and English only programs compared across a historical framework.  Emphasis on an integrated language arts approach; theories of language acquisition.  3 seminars, 1 activity.  Participation in public schools requires mandated fingerprint clearance.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '427', 'Theories, Methods, and Assessment of First and Second Language Acquisition in Schools', 'Act', '2', 'Theories, methods, materials and assessment involved in the instruction of limited English proficient (L.E.P.) students.  Bilingual, transitional, and English only programs compared across a historical framework.  Emphasis on an integrated language arts approach; theories of language acquisition.  3 seminars, 1 activity.  Participation in public schools requires mandated fingerprint clearance.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '428', 'Foundations of K-8 Literacy Teaching and Learning in Schools with Diverse Populations', 'Sem', '2', 'Introduction of foundational topics in literacy teaching and learning in K-8 schools with diverse populations.  Examination of concepts and theories underlying literacy learning and instruction, and relating research-based patterns of reading and writing development to features of the learner and learning environments.  Participation in public schools requires mandated fingerprint clearance.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '428', 'Foundations of K-8 Literacy Teaching and Learning in Schools with Diverse Populations', 'Act', '3', 'Introduction of foundational topics in literacy teaching and learning in K-8 schools with diverse populations.  Examination of concepts and theories underlying literacy learning and instruction, and relating research-based patterns of reading and writing development to features of the learner and learning environments.  Participation in public schools requires mandated fingerprint clearance.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '429', 'Learning to Teach K-8 Literacy in Schools with Diverse Populations', 'Sem', '2', 'Integration of theory and practice for planning, instruction and assessment in a balanced, comprehensive, research-based K-8 literacy and language arts program to enable children of all abilities and background to read, write, speak, listen and think effectively.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '431', 'Learning to Teach K-8 Social Studies with Diverse Populations', 'Sem', '2', 'Curriculum instruction and assessment concepts regarding teaching social studies to culturally diverse students in grades K-8, to promote civic competence and the development of elementary students knowledge and reasoning in the social sciences.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '435', 'Learning to Teach K-8 Mathematics with Diverse Populations', 'Sem', '2', 'Curriculum, instruction, and assessment concepts regarding teaching mathematics for understanding to culturally diverse students in grades K-8, with emphasis on using manipulatives to promote elementary students development of mathematical knowledge and reasoning.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '436', 'Learning to Teach K-8 Science with Diverse Populations', 'Sem', '2', 'Curriculum, instruction, and assessment concepts regarding teaching science for understanding to culturally diverse students in grades K-8, with emphasis on teaching via inquiry to promote the development of scientific knowledge and reasoning.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '438', 'Multiple Subject Clinical Practice I', 'Ind', '2', 'Clinical experiences involving observation; limited lesson planning, instruction, assessment and reflection; growth as a professional educator and participation in school-related activities in K-8 public school classrooms.  Participation in public schools requires mandated fingerprint clearance.  Total credit limited to 8 units.  Credit/No Credit grading only.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '439', 'Multiple Subject Clinical Practice Seminar I', 'Sem', '2', 'Discussion of contemporary educational issues and classroom practice; reflection on beginning clinical experiences in elementary education and their connection with educational theory and research.  Total credit limited to 4 units.  Credit/No Credit grading only.  2 seminars.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '440', 'Educating Individuals with Exceptional Needs', 'Sem', '1', 'Characteristics, incidence, and etiology of individuals with exceptional needs.  Problems, assessment, and approaches toward accommodating students with exceptional needs in the regular classroom.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '440', 'Educating Individuals with Exceptional Needs', 'Act', '1', 'Characteristics, incidence, and etiology of individuals with exceptional needs.  Problems, assessment, and approaches toward accommodating students with exceptional needs in the regular classroom.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '450', 'Teaching Performance Assessment Seminar', 'Sem', '3', 'Tasks, timelines, evaluation rubrics, and academic writing in relation to the Teaching Performance Assessments that are required for teacher credentialing in California.  Total credit limited to 2 units.  Credit/No Credit grading.  1 seminar.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '454', 'Multiple Subject Clinical Practice II', 'Ind', '2', 'Clinical experiences involving planning, instruction, assessment and reflection; growth as a professional and related activities in K-8 public school classrooms.  Participation in public schools requires mandated fingerprint clearance.  Credit/No Credit grading only.', '8');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '455', 'Multiple Subject Clinical Practice Seminar II', 'Sem', '1', 'Discussion of contemporary educational issues, state education policies and expectations for effective classroom practice; reflection on clinical experiences in elementary education and their connection with educational theory research.  Participation in public schools requires mandated fingerprint clearance.  Credit/No Credit grading.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '456', 'Multiple Subject Clinical Practice III', 'Ind', '2', 'Advanced clinical experiences involving planning, instruction, assessment and reflection; growth as a professional and related activities in K-8 public school classrooms.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '457', 'Multiple Subject Clinical Practice Seminar III', 'Sem', '2', 'Discussion of contemporary educational issues, national education policies and expectations for effective classroom practice; reflection on advanced clinical experiences in elementary education and their connection with educational theory and research.  Credit/No Credit grading.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '469', 'Part-Time Student Teaching', 'Ind', '6', 'Part-time assignment in a classroom (Single Subject only).  Includes teaching activities under the direction of a selected cooperating teacher in consultation with a university supervisor.  Assignment consists of an entire morning in the classroom (or the equivalent) for one quarter.  Credit/No Credit grading only.', '8');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '479', 'Student Teaching', 'Ind', '1', 'Full-time assignment in a classroom (Single Subject only).  Includes teaching activities under the direction of a selected cooperating teacher in consultation with a university supervisor.  Assignment consists of an entire teaching day in the school for one quarter.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '500', 'Individual Study', 'Ind', '7', 'Advanced independent study planned and completed under the direction of a member of the department faculty.  Total credit limited to 6 units.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '513', 'Education Planning and Decision Making', 'Sem', '1', 'Concepts of planning and decision making in educational administration that utilize a wide range of data gathering and analysis procedures.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '513', 'Education Planning and Decision Making', 'Act', '1', 'Concepts of planning and decision making in educational administration that utilize a wide range of data gathering and analysis procedures.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '515', 'Educational Program Management and  Evaluation', 'Sem', '1', 'Supervision, management, and evaluation of educational curriculum and educational programs.  Current trends in program management including mapping, monitoring, alignment.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '515', 'Educational Program Management and  Evaluation', 'Act', '1', 'Supervision, management, and evaluation of educational curriculum and educational programs.  Current trends in program management including mapping, monitoring, alignment.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '518', 'Administrative Services Fieldwork', 'Ind', '5', 'Supervised fieldwork in school administration for supervision at the elementary and secondary level.  Assignments must encompass three of the four academic quarters and must involve some multicultural experience.  Total credit limited to 18 units, only 9 of which may be applied toward masters degree.  Credit/No Credit grading only.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '532', 'Advanced Field Experiences in Education', 'Ind', '3', 'Advanced field experience and practical application of specialization emphasis for general and special education teachers, school administrators and school support personnel.  Total credit limited to 12 units for specialist credentials.  Total credit limited to 6 units for the masters degree.  Credit/No Credit grading only.', '2-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '542', 'Administration of Special Programs and Services', 'Sem', '1', 'Principles and practices of organizing and administering special education, reading, counseling, and other support programs.  Assessment and placement procedures, middle managements role, overview of specially funded programs, historical precedents and future trends.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '542', 'Administration of Special Programs and Services', 'Act', '1', 'Principles and practices of organizing and administering special education, reading, counseling, and other support programs.  Assessment and placement procedures, middle managements role, overview of specially funded programs, historical precedents and future trends.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '545', 'Characteristics and Instruction of Pupils with Mild/Moderate Disabilities', 'Sem', '1', 'Advanced studies and skills in educational consultation.  Emphasis on the collaborative and consultative role of the special educator with a wide range of individuals from diverse cultural backgrounds including school personnel, parents, outside agencies, and paraprofessionals.  3 seminars, 2 activities.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '545', 'Characteristics and Instruction of Pupils with Mild/Moderate Disabilities', 'Act', '1', 'Advanced studies and skills in educational consultation.  Emphasis on the collaborative and consultative role of the special educator with a wide range of individuals from diverse cultural backgrounds including school personnel, parents, outside agencies, and paraprofessionals.  3 seminars, 2 activities.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '550', 'Assessment Strategies for Special Education', 'Sem', '1', 'Using norm referenced, criterion referenced, and curriculum based testing for assessing academic, behavioral, and physical status of individuals with exceptional needs, including English language learners, for referral purposes.  Instructional and evaluation decisions regarding exceptional students in school settings.  3 seminars, 2 activities.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '550', 'Assessment Strategies for Special Education', 'Act', '1', 'Using norm referenced, criterion referenced, and curriculum based testing for assessing academic, behavioral, and physical status of individuals with exceptional needs, including English language learners, for referral purposes.  Instructional and evaluation decisions regarding exceptional students in school settings.  3 seminars, 2 activities.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '552', 'Support and Transition Strategies in Special Education', 'Sem', '1', 'Basic guidance techniques for teachers working with exceptional individuals and their families.  Career selection, preparation, and counseling.  Transition from school to work, and community resource utilization.  3 seminars, 2 activities.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '552', 'Support and Transition Strategies in Special Education', 'Act', '1', 'Basic guidance techniques for teachers working with exceptional individuals and their families.  Career selection, preparation, and counseling.  Transition from school to work, and community resource utilization.  3 seminars, 2 activities.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '556', 'Multicultural Counseling', 'Sem', '1', 'Initiation of critical analysis of personal beliefs and attitudes regarding counseling in a diverse society.  Focus on a variety of approaches to explore the beliefs and attitudes of the student in counseling settings, and examination of strategies considered effective in working with diverse populations.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '556', 'Multicultural Counseling', 'Act', '1', 'Initiation of critical analysis of personal beliefs and attitudes regarding counseling in a diverse society.  Focus on a variety of approaches to explore the beliefs and attitudes of the student in counseling settings, and examination of strategies considered effective in working with diverse populations.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '560', 'Counseling Theories', 'Sem', '1', 'Theories and practice of counseling with special emphasis on the counseling process.  Emphasis of conditions of counseling, counseling techniques, counseling diverse populations and the counselor as a professional helper.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '560', 'Counseling Theories', 'Act', '1', 'Theories and practice of counseling with special emphasis on the counseling process.  Emphasis of conditions of counseling, counseling techniques, counseling diverse populations and the counselor as a professional helper.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '564', 'Legal and Ethical Issues in Counseling', 'Sem', '1', 'Consideration of legal, ethical, cultural and related professional issues as they affect the practice of counseling.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '564', 'Legal and Ethical Issues in Counseling', 'Act', '1', 'Consideration of legal, ethical, cultural and related professional issues as they affect the practice of counseling.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '573', 'Field Experience, Counseling', 'Ind', '2', 'Practical application of guidance services and counseling in public schools, colleges and community settings.  Seminars with university staff included.  Total credit limited to 24 units.  Credit/No Credit grading only.  Maximum of 12 units may be applied toward MA Education.', '1-12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '588', 'Education, Culture, and Learning', 'Sem', '1', 'Cultural characteristics of educational institutions and practice.  Review of theory and research relating to the social and organizational context in which learning and teaching takes place.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '589', 'Educational Research Methods', 'Sem', '2', 'Introduction to research methodologies, application of inferential and descriptive statistics, critical analysis of research designs and data collection techniques.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '589', 'Educational Research Methods', 'Act', '2', 'Introduction to research methodologies, application of inferential and descriptive statistics, critical analysis of research designs and data collection techniques.  3 seminars, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '590', 'Research Application in Education', 'Sem', '1', 'Application of social science research techniques to problems in education and human services.  Capstone experience for the School of Education masters inquiry course sequence.  Completion of an inquiry project required.  2 seminars, 2 activities.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', '590', 'Research Application in Education', 'Act', '1', 'Application of social science research techniques to problems in education and human services.  Capstone experience for the School of Education masters inquiry course sequence.  Completion of an inquiry project required.  2 seminars, 2 activities.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', 'P300', 'Historical, Philosophical, and Social Foundations of Public Education', 'Lec', '1', 'Historical, philosophical, and social foundations of public education in relation to school curriculum, instruction and dispositions of effective teachers.  Structured observation and participation in K-12 public schools with attention to instructional practices for diverse learners.  2 lectures, 1 activity.  Participation in public schools requires mandated fingerprint clearance.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', 'P300', 'Historical, Philosophical, and Social Foundations of Public Education', 'Act', '1', 'Historical, philosophical, and social foundations of public education in relation to school curriculum, instruction and dispositions of effective teachers.  Structured observation and participation in K-12 public schools with attention to instructional practices for diverse learners.  2 lectures, 1 activity.  Participation in public schools requires mandated fingerprint clearance.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', 'P423', 'Bilingual Literacy', 'Sem', '1', 'Patterns of classroom organization, application of reading programs, approaches, methods in English and Spanish, and supervised field experiences in elementary classrooms with bilingual students.  3 seminars, 1 activity.  Limited to students seeking BCLAD certification.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EDUC', 'P423', 'Bilingual Literacy', 'Act', '1', 'Patterns of classroom organization, application of reading programs, approaches, methods in English and Spanish, and supervised field experiences in elementary classrooms with bilingual students.  3 seminars, 1 activity.  Limited to students seeking BCLAD certification.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '112', 'Electric Circuit Analysis I', 'Lec', '1', 'Introduction to basic circuit analysis.  Resistive circuits, voltage and current sources, network theorems.  Course may be offered in classroom-based or online format.  2 lectures.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '200', 'Special Problems', 'Ind', '2', 'Supervised investigation, including a written report, of a topic chosen with prior approval of instructor.  Total credit limited to 4 units.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '201', 'Electric Circuit Theory', 'Lec', '1', 'Application of fundamental circuit laws and theorems to the analysis of DC, and steady-state single-phase and three-phase circuits.  Not for electrical engineering majors.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '211', 'Electric Circuit Analysis II', 'Lec', '3', 'Continuation of basic circuit analysis.  Op-amp circuits.  Energy storage elements, RC and RL circuits, and AC steady state analysis.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '212', 'Electric Circuit Analysis III', 'Lec', '4', 'AC power, 3-phase circuits.  Mutual inductance, series and parallel resonance and two-port networks.  Frequency response, including Bode plots.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '228', 'Continuous-Time Signals and Systems', 'Lec', '1', 'Continuous-time systems analysis, with emphasis on linear time-invariant (LTI) systems.  Classifications of continuous-time systems.  Convolution and its application to LTI systems.  The Laplace transform, Fourier transform, and Fourier series, and their application to the analysis of LTI systems.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '241', 'Electric Circuit Analysis Laboratory II', 'Lab', '5', 'Use of electrical and electronic test equipment.  Experimental verification of circuit analysis concepts including Kirchhoffs Laws, Thevenins Theorem, maximum power transfer and superposition.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '242', 'Electric Circuit Analysis Laboratory III', 'Lab', '6', 'Observation of transient and steady-state phenomena, phase-shift circuits, resonance.  Use of phasor diagrams.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '251', 'Electric Circuits Laboratory', 'Lab', '6', 'Techniques of measurement of DC and steady-state AC circuit parameters.  Equivalent circuits, nonlinear elements, resonance.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '255', 'Energy Conversion Electromagnetics', 'Lec', '1', 'Fundamentals of electro-mechanical energy conversion.  Magnetic circuits and electromagnetic devices.  Theory of operation and operating characteristics of transformers, and AC induction and synchronous machines.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '295', 'Energy Conversion Electromagnetics Laboratory', 'Lab', '2', 'Single-phase and three-phase transformers.  Starting of rotating machines, evaluation of characteristics of rotating machines.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '302', 'Classical Control Systems', 'Lec', '4', 'Introduction to feedback control systems.  System modeling.  Transfer functions.  Graphical system representation.  System time response, stability.  Root Locus.  Frequency response.  Compensation.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '306', 'Semiconductor Device Electronics', 'Lec', '1', 'Internal operation, semiconductor physics, terminal characteristics, models and application of diodes (LEDs, solar cells, and photo-diodes) and transistors (field-effect and bipolar).  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '307', 'Digital Electronics and Integrated Circuits', 'Lec', '6', 'Analysis, design, application and interfacing of integrated logic circuits, including NMOS, CMOS, TTL, ECL, and other logic families.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '314', 'Introduction to Communication Systems', 'Lec', '4', 'Analog modulation, including:  double-sideband modulation, amplitude modulation, single-sideband modulation, frequency modulation, phase modulation.  Performances of such systems in the presence of white Gaussian noise.  Implementations of transmitters and receivers.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '321', 'Electronics', 'Lec', '5', 'Semiconductor devices and circuits.  Instrumentation amplifiers, power control rectifiers, feedback, pulse circuits, digital logic circuits.  Not for Electrical Engineering majors.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '322', 'Microcontrollers for Everyone', 'Lec', '1', 'Microcontroller history and computer systems overview.  Introduction to basic electrical circuits and computer programming concepts.  Overview of computer peripherals such as LEDs, switches, LCD displays, timers, and ADCs; and interfacing various types of external sensors.  Developing applications of microcontrollers using an integrated development environment.  3 lectures, 1 laboratory.  Fulfills GE Area F.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '322', 'Microcontrollers for Everyone', 'Lab', '1', 'Microcontroller history and computer systems overview.  Introduction to basic electrical circuits and computer programming concepts.  Overview of computer peripherals such as LEDs, switches, LCD displays, timers, and ADCs; and interfacing various types of external sensors.  Developing applications of microcontrollers using an integrated development environment.  3 lectures, 1 laboratory.  Fulfills GE Area F.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '328', 'Discrete Time Signals and Systems', 'Lec', '1', 'Discrete-time systems and analysis, with emphasis on linear time-invariant (LTI) systems.  Sampling theorem.  Classification of discrete-time systems.  Convolution and its application to LTI systems.  The z transform, discrete-time Fourier transform, and discrete Fourier transform.  Introduction to digital filters.  3 lectures.  Crosslisted as CPE/EE 328.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '342', 'Classical Control Systems Laboratory', 'Lab', '6', 'Laboratory work pertaining to classical control systems, including servo control, transient and frequency responses, stability, and computer-aided analysis of control systems.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '346', 'Semiconductor Device Electronics Laboratory', 'Lab', '2', 'Experimental determination of device characteristics and models.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '347', 'Digital Electronics and Integrated Circuits Laboratory', 'Lab', '10', 'Computer simulation and experimental investigation of the characteristics, applications and interfacing of different logic families.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '361', 'Electronics Laboratory', 'Lab', '6', 'Instrumentation amplifiers, feedback, rectifiers and power control, pulse and digital logic circuits.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '368', 'Signals and Systems Laboratory', 'Lab', '2', 'Laboratory work pertaining to linear systems, including Fourier analysis, time and frequency responses, and system transfer function.  1 laboratory.  Crosslisted as CPE/EE 368.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '400', 'Special Problems', 'Ind', '10', 'Supervised investigation, including a written report, of a topic chosen with prior approval of instructor.  Total credit limited to 4 units.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '402', 'Electromagnetic Waves', 'Lec', '1', 'Maxwells equations and plane wave propagation in materials.  Reflection and transmission of normal and oblique incidence plane waves at planar boundaries between different media.  Wave guides.  Antennas.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '407', 'Power Systems Analysis II', 'Lec', '1', 'Symmetrical components, unbalanced faults, power system stability, system protection, relays and relay systems, power system instrumentation and measurement techniques, economic operation.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '409', 'Electronic Design', 'Lec', '1', 'Design of electronic systems and subsystems using analog and digital integrated circuits.  Design principles and techniques.  Analysis and design of feedback amplifiers; operational amplifier applications.  Design of analog/digital and digital/analog converters.  Power supply design.  Emphasis on IC implementation.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '411', 'Power Electronics II', 'Lec', '1', 'Switching losses.  Analysis, performance characterization, and design of snubber circuits and resonant converters.  Operation of DC transmission lines, flexible AC transmission system (FACTS) controllers, three-phase inverters, and AC motor drives.  Use of commercially available software.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '411', 'Power Electronics II', 'Lab', '2', 'Switching losses.  Analysis, performance characterization, and design of snubber circuits and resonant converters.  Operation of DC transmission lines, flexible AC transmission system (FACTS) controllers, three-phase inverters, and AC motor drives.  Use of commercially available software.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '412', 'Advanced Analog Circuits', 'Lec', '1', 'Application of linear integrated circuits to data acquisition problems:  transducer interfacing, linear and nonlinear preprocessing, phase-locked loops, and high performance quantization and recovery (A/D, D/A conversion).  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '419', 'Digital Signal Processing', 'Lec', '1', 'Review of Z-transform, convolution and discrete Fourier Transform.  Digital filter design.  Fast Fourier Transform.  Theory and applications of digital signal processors.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '420', 'Sustainable Electric Energy Conversion', 'Lec', '1', 'Electrical engineering aspects of photovoltaic and wind power generation and usage, and electrochemical energy conversion.  Power control, processing, and quality for grid-connected and stand-alone systems.  Distribution and storage of electric energy.  Hydrogen and synthetic fuels.  Distributed generation.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '420', 'Sustainable Electric Energy Conversion', 'Lab', '2', 'Electrical engineering aspects of photovoltaic and wind power generation and usage, and electrochemical energy conversion.  Power control, processing, and quality for grid-connected and stand-alone systems.  Distribution and storage of electric energy.  Hydrogen and synthetic fuels.  Distributed generation.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '422', 'Polymer Electronics Laboratory', 'Lab', '1', 'Experimental procedures in polymer electronics.  Investigation of the characteristics of a polymer electronic device.  1 laboratory.  Crosslisted as EE/PHYS 422.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '440', 'Wireless Communications', 'Lec', '1', 'Wireless microwave system design and analysis.  RF transmission lines, microwave networks, receiver design, modulation techniques, and mixer characterization and realizations.  Noise and distortion, RF oscillators and frequency synthesizers, filter design.  Radiating systems and electromagnetic wave propagation, microwave amplifier design.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '449', 'Electronic Design Laboratory', 'Lab', '2', 'Design of electronic systems and subsystems using integrated circuits.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '452', 'Advanced Analog Circuits Laboratory', 'Lab', '2', 'Advanced laboratory study of LC and VCO oscillators, phase detectors, phase-locked loop circuits, transducer interface circuits, noise sources and signal-to-noise determination, ADC and DAC for data conversion.  Formal experiments and computer SPICE simulation.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '459', 'Digital Signal Processing Laboratory', 'Lab', '2', 'Experiments in digital filter design and digital signal processing emphasizing various areas of application.  Formal experiments and individual project work, including DSP algorithm and digital filter analysis, design and implementation using Matlab, and real-time implementations using C on an embedded DSP processor.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '460', 'Senior Project Preparation', 'Lec', '1', 'Introduction to teamwork and team-oriented project execution.  Project planning, scheduling and analysis.  Usage of tools for project management including Gantt and Pert Charts.  Project development, cost and time estimation using top-down and bottom-up approaches.  Ethics and ethical issues as they pertain to the conduct of engineering.  Development of senior project proposal.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '460', 'Senior Project Preparation', 'Lab', '1', 'Introduction to teamwork and team-oriented project execution.  Project planning, scheduling and analysis.  Usage of tools for project management including Gantt and Pert Charts.  Project development, cost and time estimation using top-down and bottom-up approaches.  Ethics and ethical issues as they pertain to the conduct of engineering.  Development of senior project proposal.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '461', 'Senior Project I', 'Ind', '24', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '462', 'Senior Project II', 'Ind', '22', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '480', 'Wireless Communications Laboratory', 'Lab', '2', 'Wireless microwave system design and analysis.  RF transmission lines, microwave networks, receiver design, modulation techniques, and mixer characterization and realizations.  Noise and distortion, RF oscillators and frequency synthesizers, filter design.  Radiating systems and electromagnetic wave propagation, microwave amplifier design.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '494', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '495', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '500', 'Individual Study', 'Ind', '10', 'Advanced independent study planned and completed under the direction of a member of the department faculty.  Total credit limited to 6 units.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '511', 'Electric Machines Theory', 'Sem', '1', 'Advanced topics in electric machines theory.  Introduction to Parks transformation.  Analysis of electric machines using Krons generalized concept.  Vector control of induction machines.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '513', 'Control Systems Theory', 'Sem', '1', 'State representation of dynamic systems.  Mathematical models of physical devices, controllability and observability.  Design of closed-loop systems.  Optimal control theory.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '526', 'Advanced Digital Communications', 'Sem', '1', 'Modern digital communication systems.  M-ary signals.  Vector space representation of signals.  Optimum receiver principles.  Common signal sets.  Signal space dimensionality versus time-bandwidth product.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '529', 'Microwave Device Electronics', 'Sem', '1', 'Emphasis on device theory of operation, fabrication techniques and circuit principles of active microwave solid-state devices, their noise aspects and systems applications.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '530', 'Fourier Optics', 'Sem', '1', 'Approach to the design and analysis of optical systems using linear communication theory, including Fourier analysis.  Analysis of two-dimensional signals and systems, foundations of scalar diffraction theory.  Fresnel and Fraunhofer diffraction.  Wave-optics analysis of coherent optical systems, frequency analysis of optical imaging systems, holo-graphy.4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '541', 'Advanced Microwave Laboratory', 'Lab', '1', 'Experimental measurement in waveguide and microstrip circuits employing the advanced Network Analyzer.  Design of both passive and active microwave circuits using microstrip.  Graphical and analytical design techniques as well as the use of computer-aided design codes.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '563', 'Graduate Seminar', 'Sem', '1', 'Seminar designed as a culminating component to the Master of Public Policy Program.  Individual research under the supervision of the faculty within a small discussion environment, leading to a graduate project or paper that demonstrates practical mastery of the MPP curriculum.  Total credit limited to 8 units.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '594', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '595', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', '599', 'Design Project (Thesis)', 'Ind', '22', 'Each individual or group will be assigned a project for solution under faculty supervision as a requirement for the masters degree, culminating in a written report/thesis.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', 'P306', 'Semiconductor Device Electronics', 'Lec', '1', 'Internal operation, semiconductor physics, terminal characteristics, models and application of diodes (LEDs, solar cells, and photo-diodes) and transistors (field-effect and bipolar).  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', 'P307', 'Digital Electronics and Integrated Circuits', 'Lec', '1', 'Analysis, design, application and interfacing of integrated logic circuits, including NMOS, CMOS, TTL, ECL, and other logic families.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', 'P321', 'Electronics', 'Lec', '2', 'Semiconductor devices and circuits.  Instrumentation amplifiers, power control rectifiers, feedback, pulse circuits, digital logic circuits.  Not for Electrical Engineering majors.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', 'P346', 'Semiconductor Device Electronics Laboratory', 'Lab', '1', 'Experimental determination of device characteristics and models.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', 'P347', 'Digital Electronics and Integrated Circuits Laboratory', 'Lab', '1', 'Computer simulation and experimental investigation of the characteristics, applications and interfacing of different logic families.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', 'P361', 'Electronics Laboratory', 'Lab', '1', 'Instrumentation amplifiers, feedback, rectifiers and power control, pulse and digital logic circuits.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', 'P400', 'Special Problems', 'Ind', '2', 'Supervised investigation, including a written report, of a topic chosen with prior approval of instructor.  Total credit limited to 4 units.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('EE', 'P407', 'Power Systems Analysis II', 'Lec', '1', 'Symmetrical components, unbalanced faults, power system stability, system protection, relays and relay systems, power system instrumentation and measurement techniques, economic operation.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '103', 'Writing and Rhetoric Tutorial', 'Lab', '1', 'Directed, weekly practice in writing in a laboratory environment.  Open to all students enrolled in ENGL 102, ENGL 133, or ENGL 134; required of all students scoring below 147 on the English Placement Test (EPT).  Students scoring below 147 must enroll in the Writing and Rhetoric Stretch Program and take ENGL 103 during the second quarter of the stretch class, concurrently with ENGL 133 or ENGL 134.  Not for baccalaureate credit.  Credit/No Credit grading only.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '133', 'Writing & Rhetoric for English as a Second Language Students', 'Lec', '2', 'Rhetorical principles and tactics applied to written work.  Writing as a recursive process that leads to greater organizational coherency, stylistic complexity, and rhetorical awareness with an emphasis on grammatical elements appropriate for English as a Second Language students.  4 lectures.  Fulfills GE A1.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '134', 'Writing and Rhetoric', 'Lec', '44', 'Rhetorical principles and tactics applied to written work.  Writing as a recursive process that leads to greater organizational coherency, stylistic complexity, and rhetorical awareness.  4 lectures.  Fulfills GE A1.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '145', 'Reasoning, Argumentation, and Writing', 'Lec', '31', 'The principles of reasoning in argumentation.  Examination of rhetorical principles and responsible rhetorical behavior.  Application of these principles to written and oral communications.  Effective use of research methods and sources.  4 lectures.  Crosslisted as COMS/ENGL/HNRS 145.  Fulfills GE A3.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '148', 'Reasoning, Argumentation and Professional Writing', 'Lec', '3', 'The principles of reasoning in professional writing.  Discussion and application of rhetorical principles, both oral and written, in professional environments.  Study of methods, resources and common formats used in corporate or research writing.  4 lectures.  Crosslisted as ENGL/HNRS 148.  Fulfills GE A3.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '149', 'Technical Writing for Engineers', 'Lec', '25', 'The principles of technical writing.  Discussion and application of rhetorical principles in technical environments.  Study of methods, resources and common formats used in corporate or research writing.  4 lectures.  Crosslisted as ENGL/HNRS 149.  Fulfills GE A3.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '150', 'Writing Tutorial', 'Act', '1', 'Guided discussion and practice of writing strategies for students seeking support for writing-related coursework and/or the GWR.  Weekly, individualized and group sessions with a peer writing consultant offering feedback based on the audience, purpose, and context of a writing task.  Credit/No Credit only.  Total credit limited to 4 units.  1 activity.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '202', 'Introduction to Literary Studies', 'Lec', '1', 'Introduction to literary genres, concepts, and terms.  Emphasis on explication and interpretation, and on writing about literature.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '203', 'Core I: 450-1485', 'Lec', '1', 'Representative canonical and non-canonical readings in the literature of the period.  Selections may include such readings as Beowulf, Chaucer, Dante, a mystery or morality play, the Pearl Poet and others, as chosen by the instructor.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '204', 'Core II: 1485-1660', 'Lec', '2', 'Representative canonical and non-canonical readings in the literature of the period.  Selections may include such readings as Bradstreet, Donne, Milton, Shakespeare, Spenser, and others, as chosen by the instructor.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '230', 'Masterworks of British Literature through the Eighteenth Century', 'Lec', '3', 'Covers a thousand years of British literature, from the eighth to the eighteenth century and may include such readings as Beowulf, The Canterbury Tales, Utopia, Othello, Paradise Lost, Oroonoko and Gullivers Travels.  4 lectures.  Fulfills GE C1.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '231', 'Masterworks of British Literature from the Late 18th Century to the Present', 'Lec', '4', 'Broadly surveys Romantic, Victorian, Modern, and Contemporary British literature in an historical-cultural context.  Investigates works from several genres and a variety of national and cultural voices.  May include such writers as Wordsworth, Wollstonecraft, Dickens, G.  Eliot, Wilde, Woolf, Yeats, and Gordimer.  4 lectures.  Crosslisted as ENGL 231/HNRS 232.  Fulfills GE C1.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '240', 'The American Tradition in Literature', 'Lec', '2', 'A broadly based survey of American literature, exploring the impact of various world cultures on the evolving definition of the American experience.  Literary expression of movements that shape the American character over time, such as Puritanism, Transcendentalism, and Naturalism.  4 lectures.  Fulfills GE C1.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '251', 'Great Books I: Introduction to Classical Literature', 'Lec', '6', 'Examination of the ancient epics and classical literature of Mesopotamia, Greece, and Rome.  May include such readings as The Epic of Gilgamesh, the Iliad, the Odyssey, Genesis, Exodus, Antigone, the Symposium, the Aeneid, and Marcus Aureliuss Meditations.  4 lectures.  Crosslisted as ENGL/HNRS 251.  Fulfills GE C1.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '252', 'Great Books II: Medieval to Enlightenment Literature', 'Lec', '3', 'Examination of key works marking the transition from Mediterranean Classicism (c.  500 CE) to an emergent European tradition (c.  1800 CE).  May include such readings as Augustines Confessions, Song of Roland, Egils Saga, the Consolation of Philosophy, The Romance of Tristan, the Inferno, Cellinis Autobiography, Utopia, Princess of Cleves, Candide, Discourse on Method, and Rousseaus Confessions.  4 lectures.  Fulfills GE C1.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '253', 'Great Books III: Romanticism to Modernism Literature', 'Lec', '5', 'Examination of key works marking the Romantic Revolution and the realist and modernist movements that followed in its wake.  May include such readings as the poetry of Blake, Wordsworth, Eliot, Rimbaud, Plath, Ginsberg, and Stein; Notes from Underground, The Death of Ivan Ilych, The Metamorphosis and/or The Hunger Artist, Heart of Darkness, Sonnys Blues, and Virginia Woolfs short fiction and essays.  4 lectures.  Fulfills GE C1.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '290', 'Introduction to Linguistics', 'Lec', '1', 'Introduction to the nature of language; concepts and methods of linguistic science.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '302', 'Writing: Advanced Composition', 'Lec', '1', 'Writing and analysis of expository and argumentative papers at an advanced level.  Special attention paid to issues of style and voice.  Critical reading of models of effective writing.  4 lectures.  Fulfills GWR.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '303', 'Core IV: 1789-1861', 'Lec', '1', 'Representative canonical and non-canonical readings in the literature of the period.  May include such authors as Austen, Emerson, Hawthorne, Keats, Wordsworth, and others, as chosen by the instructor.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '304', 'Core V: 1861-1914', 'Lec', '2', 'Representative canonical and non-canonical readings in the literature of the period.  May include such authors as Arnold, Dickinson, James, Tennyson, Whitman, and others, as chosen by the instructor.  4 lecture.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '310', 'Corporate Communication', 'Lec', '6', 'Instruction and practice in forms of communication characteristic of business and industry.  4 lectures.  Fulfills GWR.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '317', 'Technical Editing', 'Lec', '1', 'Instruction and practice in editing skills commonly used in workplace settings.  Includes practical instruction in copyediting, sentence level editing, and substantive editing for accuracy and consistency.  Editing documents, illustrations, web pages for consistency and use.  Application of grammar and punctuation.  4 lectures.  Fulfills GWR.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '339', 'Introduction to Shakespeare', 'Lec', '3', 'Shakespeares works as texts, productions and major historical, aesthetic and cultural touchstones.  The authors intellectual and social influences on four centuries of theatre and his subsequent impact on literature and other arts.  4 lectures.  Fulfills GE C4 except for English majors.  Fulfills GWR for students with junior standing (90 units).', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '347', 'African American Literature', 'Lec', '4', 'The writings of African Americans from the end of the eighteenth century to the present.  Individual works and literary trends among African Americans of various periods and contexts:  intellectual, political, and cultural.  4 lectures.  Crosslisted as ENGL/HNRS 347.  Fulfills GE C4 except for English majors.  Fulfills USCP.  Fulfills GWR for students with junior standing (90 units).', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '350', 'The Modern Novel', 'Lec', '2', 'Readings in the modern novel in its historical and cultural context.  4 lectures.  Fulfills GE C4 except for English majors.  Fulfills GWR for students with junior standing (90 units).', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '352', 'Modern Drama', 'Lec', '1', 'Reading and analysis of world drama of the last 150 years, thereby enhancing student awareness of modern culture, history, ethics, politics, and the human condition.  Design work, multi-media forms, art, music, and cinema as components or informing elements of the works under consideration.  4 lectures.  Fulfills GE C4 except for English majors.  Fulfills GWR for students with junior standing (90 units).', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '361', 'Reading Instruction for the Teaching of Young Adult Literature', 'Lec', '1', 'Analysis and evaluation of young adult literature appropriate for classroom instruction in grades 6-12.  Special attention paid to metacognitive strategies of making sense of text with an emphasis on pedagogical approaches.  Twenty hours of fieldwork in secondary schools required.  Participation in public schools requires mandated fingerprint clearance.  4 lectures, 1 activity.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '361', 'Reading Instruction for the Teaching of Young Adult Literature', 'Act', '1', 'Analysis and evaluation of young adult literature appropriate for classroom instruction in grades 6-12.  Special attention paid to metacognitive strategies of making sense of text with an emphasis on pedagogical approaches.  Twenty hours of fieldwork in secondary schools required.  Participation in public schools requires mandated fingerprint clearance.  4 lectures, 1 activity.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '368', 'Theory and Practice of Peer-to-Peer Writing Instruction', 'Lec', '1', 'Discussion and application of theories and practices central to writing center work, such as collaborative learning, the writing process, social dimensions of the peer/tutor relationship, and strategies for working with specific student populations including second-language writers and writers from across the disciplines.  Required for those interested in becoming tutors in the University Writing and Rhetoric Center and/or new teaching assistants in English.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '368', 'Theory and Practice of Peer-to-Peer Writing Instruction', 'Act', '1', 'Discussion and application of theories and practices central to writing center work, such as collaborative learning, the writing process, social dimensions of the peer/tutor relationship, and strategies for working with specific student populations including second-language writers and writers from across the disciplines.  Required for those interested in becoming tutors in the University Writing and Rhetoric Center and/or new teaching assistants in English.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '371', 'Film Styles and Genres', 'Lec', '1', 'Major films within particular cinematic genres or styles, with emphasis on critical interpretation, aesthetic appreciation, and the films historical and cultural contexts.  The Schedule of Classes will list topic selected.  Total credit limited to 12 units.  3 lectures, 1 laboratory.  Fulfills GE C4 except for English majors.  Fulfills GWR for students with junior standing (90 units).', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '371', 'Film Styles and Genres', 'Lab', '1', 'Major films within particular cinematic genres or styles, with emphasis on critical interpretation, aesthetic appreciation, and the films historical and cultural contexts.  The Schedule of Classes will list topic selected.  Total credit limited to 12 units.  3 lectures, 1 laboratory.  Fulfills GE C4 except for English majors.  Fulfills GWR for students with junior standing (90 units).', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '372', 'Film Directors', 'Lec', '3', 'Films of one or more major film directors, with emphasis on critical interpretation, aesthetic appreciation, and the films historical and cultural contexts.  The Schedule of Classes will list topic selected.  Total credit limited to 12 units.  3 lectures, 1 laboratory.  Fulfills GE C4 except for English majors.  Fulfills GWR for students with junior standing (90 units).', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '372', 'Film Directors', 'Lab', '3', 'Films of one or more major film directors, with emphasis on critical interpretation, aesthetic appreciation, and the films historical and cultural contexts.  The Schedule of Classes will list topic selected.  Total credit limited to 12 units.  3 lectures, 1 laboratory.  Fulfills GE C4 except for English majors.  Fulfills GWR for students with junior standing (90 units).', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '387', 'Fiction Writing', 'Lec', '2', 'How to write and read fiction.  Exploring and understanding the elements of fiction writing, employing models by established writers.  Total credit limited to 8 units.  4 lectures.  Fulfills GE C4 except for English majors.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '388', 'Poetry Writing', 'Lec', '2', 'How to write and read poetry.  Exploring a variety of formal options, employing model poems by established writers and identifying and enhancing what is best in poetry written in class.  Total credit limited to 8 units.  4 lectures.  Fulfills GE C4 except for English majors.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '391', 'Topics in Applied Linguistics', 'Lec', '1', 'Topics in applied linguistics including sociolinguistics, first and second language acquisition, literacy, bilingualism, and dialectology.  Applications to teaching the English language.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '395', 'History of the English Language', 'Lec', '1', 'Linguistic approach to the history of the English language:  evolution of phonology, morphology, lexicon, syntax, and semantics within the changing cultural context of the last 2000 years.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '2', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '408', 'Internship', 'Ind', '1', 'Part-time work experience in the entertainment industry.  Ability to work independently; strong verbal and written skills.  Faculty approval of job position required.  Evaluations by job supervisor and written reports by student required.  120 hours of work experience.  Total credit limited to 8 units.  Credit/No Credit grading.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '439', 'Significant British Writers', 'Sem', '1', 'Selected British writers, as individual writers or in groups.  The Schedule of Classes will list topic selected.  Total credit limited to 12 units.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '449', 'Significant American Writers', 'Sem', '1', 'Selected American writers, as individual writers or in groups.  The Schedule of Classes will list topic selected.  Total credit limited to 12 units.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '459', 'Significant World Writers', 'Sem', '1', 'Selected world writers as individual writers or in groups.  The Schedule of Classes will list topic selected.  Total credit limited to 12 units.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '461', 'Senior Project', 'Ind', '1', 'Completion, under faculty supervision, of an investigative project typical of problems which graduates must solve in their fields of employment.  Required minimum of 90 hours.  Analytical, formal report is required.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '487', 'Advanced Creative Writing: Fiction', 'Lec', '1', 'Instruction and practice in advanced writing, revising and evaluating of fiction.  Total credit limited to 8 units.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '488', 'Advanced Creative Writing: Poetry', 'Lec', '1', 'Instruction and practice in advanced writing, revising and evaluating of poetry.  Total credit limited to 8 units.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '498', 'Approaches to Teaching English as a Second Language/Dialect', 'Lec', '1', 'Approaches to teaching English as a second language.  Attention to materials development and testing.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '502', 'Seminar in Critical Analysis', 'Sem', '1', 'Basic approaches used by critics.  Multiple points of view.  Application to literary works.  Class Schedule will list topic selected.  Total credit limited to 8 units.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '503', 'Graduate Introduction to Linguistics', 'Sem', '1', 'Introduction to linguistics for graduate students.  Phonology, morphology lexicon, syntax, and variation within language; application of linguistics to real-world issues.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '511', 'Seminar in American Literary Periods', 'Sem', '1', 'American periods.  Written and oral reports of individual investigation.  The Schedule of Classes will list topic selected.  Total credit limited to 20 units.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGL', '515', 'Apprenticeship in Teaching Literature, Composition, or Linguistics at College Level', 'Ind', '1', 'Supervised experience in planning, teaching, and evaluating a 100-, 200- or 300-level linguistics, composition, or literature class taught by English faculty member.  Planning, selecting texts, conferring with students, discussing and constructing assignments, lecturing, leading small group discussions.  Credit/No Credit grading only.  Total credit limited to 8 units.', '2');
