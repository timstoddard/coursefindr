INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASCI', '420', 'Animal Metabolism and Nutrition', 'Lec', '1', 'Metabolism of proteins, carbohydrates, lipids, minerals, vitamins and water, and the relationship of nutrient utilization to animal production.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASCI', '450', 'Computer Applications in Animal Science: Spreadsheet Analysis', 'Lec', '1', 'Development of spreadsheets relating to livestock production.  Integration of database and analytical techniques.  Cost-benefit analyses of livestock production systems.  2 lectures, 2 activities.', '4');
