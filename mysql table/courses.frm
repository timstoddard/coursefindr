INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('MU', '227', 'Popular Music of the USA', 'Lec', '1', 'Study of popular music of USA from the nineteenth century to contemporary times.  Emphases include understanding of cultural identity, social diversity, musical aesthetics, and artistic significance.  Investigations engage ethnic relations, gender issues, religious expression, performance practice, technological development, modernization, commodification, and political empowerment.  3 lectures, 1 activity.  Fulfills GE C3 and USCP.', '4');
