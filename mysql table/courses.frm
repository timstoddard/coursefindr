INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '551', 'Architectural Design', 'Lab', '1', 'Professional initiative and responsibility in integrating architectural design theory and practice with fields influencing the total environment.  Building types considered as the coordinating factor.  Total credit limited to 15 units with no more than 5 units in any one quarter.  5 laboratories.', '5');
