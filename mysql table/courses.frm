INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASCI', '477', 'Senior Project - Research Experience in Animal Science', 'Ind', '10', 'Independent research experience in a specific area of animal science conducted under faculty supervision.  Satisfies senior project requirement.  Minimum 90 hours required.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASCI', '478', 'Senior Project - Advanced Internship Experience in Animal Science', 'Ind', '10', 'Independent internship experience conducted under faculty supervision focusing on a discipline area of animal science.  Completion of a project as a component of the internship.  Satisfies senior project requirement.  Minimum 90 hours required.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASCI', '479', 'Senior Project - Current Topics in Animal Science', 'Lec', '1', 'Critical evaluation and formal presentation of current issues facing animal agriculture.  Evaluation of current topics, analysis of supporting evidence and logic, and synthesis and formal presentation of the resulting perspectives on different approaches to current challenges.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASCI', '490', 'Advanced Animal Production and Management Enterprise', 'Lec', '5', 'Advanced field experience in animal production systems.  May include health, nutrition, reproduction, management, processing, budgeting, and/or marketing exercises as well as management decision-making opportunities.  Total major credit for ASCI 490 limited to 6 units.  Total credit for ASCI 490 limited to 10 units.  Credit/No Credit grading only.  1-5 lectures.', '1-5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASCI', '500', 'Individual Study in Animal Science', 'Ind', '8', 'Advanced independent study planned and completed under the direction of a member of the Animal Science faculty.  Total credit limited to 6 units.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASCI', '581', 'Graduate Seminar in Animal Science', 'Sem', '1', 'Current findings and research problems in the field and their application to the industry.  Credit/No Credit grading only.  Total credit limited to 12 units.  1-4 seminars.', '1-4');
