INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASCI', '412', 'Advanced Livestock Event Planning', 'Lec', '1', 'Organization and planning for the Western Bonanza Junior Livestock Show.  Establishment of committee assignments and show manager responsibilities.  Corporate partnerships established and fund raising begun.  Planning for activities and guest speakers and new student recruitment.  Total credit limited to 6 units.  1 lecture, 2 activities.', '3');
