INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('NR', '247', 'Forest Surveying', 'Lec', '1', 'Use and care of tapes, staff compass, abney levels, total stations, and GPS receivers.  Keeping field notes, measurements by tape.  Closed and open traverse by compass and total stations.  Turning angles and determining directions of lines.  Map reading and public land description.  GPS measurements.  Weekend field trips required.  1 lecture, 1 laboratory.  Crosslisted as BRAE/NR 247.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('NR', '247', 'Forest Surveying', 'Lab', '1', 'Use and care of tapes, staff compass, abney levels, total stations, and GPS receivers.  Keeping field notes, measurements by tape.  Closed and open traverse by compass and total stations.  Turning angles and determining directions of lines.  Map reading and public land description.  GPS measurements.  Weekend field trips required.  1 lecture, 1 laboratory.  Crosslisted as BRAE/NR 247.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('NR', '290', 'Intercollegiate Forestry Activities', 'Lab', '1', 'Beginning through advanced skills in the event areas of college forestry activities.  Instruction in use of specialized equipment and safety.  Minimum of 4 hours of laboratory per week.  Total credit limited to 18 units.  Credit/No Credit grading only.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('NR', '306', 'Natural Resource Ecology and Habitat Management', 'Lec', '1', 'Resource ecology and management implications in the major ecosystems of North America.  Importance of maintaining the natural dynamics of energy flow and nutrient cycles at the community and ecosystem level to sustain uses and values.  Humanitys role as a principal factor of change of the resources in natural systems.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('NR', '306', 'Natural Resource Ecology and Habitat Management', 'Lab', '2', 'Resource ecology and management implications in the major ecosystems of North America.  Importance of maintaining the natural dynamics of energy flow and nutrient cycles at the community and ecosystem level to sustain uses and values.  Humanitys role as a principal factor of change of the resources in natural systems.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('NR', '308', 'Fire and Society', 'Lec', '2', 'Prehistorical and historical record of human use of and attitude toward fire.  Mythology and religion of fire.  Traditional, cultural and ethnic variations and their influence on modern U.S.  institutions involved in managing fire.  3 lectures, 1 activity.  Crosslisted as ES/NR 308.  Fulfills GE D5.', '4');
