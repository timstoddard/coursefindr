INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASTR', '101', 'Introduction to the Solar System', 'Lec', '1', 'Descriptive astronomical properties of the Earth, Moon, other planets and their satellites. Comets, asteroids and other members of the Solar System. Theories of the formation of the Solar System. Opportunities for telescope observations. Not open to students who have completed or are taking ASTR 301, ASTR 302, or PHYS 132. 4 lectures. Fulfills GE B3.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASTR', '102', 'Introduction to Stars and Galaxies', 'Lec', '1', 'Descriptive astronomical properties of the Sun, stars, galaxies and interstellar material. Expanding universe and cosmological models. Opportunities for telescope observations. Not open to students who have completed or are taking ASTR 112, ASTR 301, ASTR 302, or PHYS 132. 4 lectures. Fulfills GE B3.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASTR', '301', 'The Solar System', 'Lec', '1', 'Quantitative and descriptive properties of the Solar System including the physics of the planets, their satellites, comets and interplanetary media.  Possible origins of the Solar System.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ASTR', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '2', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '111', 'General Biology', 'Lec', '3', 'Principles of cellular biology, heredity, ecology, biological diversity, and evolution, with emphasis on their relationships to human affairs. Not open to students who have completed BIO 115 or BIO 161. 3 lectures, 1 laboratory. Fulfills GE B2 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '111', 'General Biology', 'Lab', '16', 'Principles of cellular biology, heredity, ecology, biological diversity, and evolution, with emphasis on their relationships to human affairs. Not open to students who have completed BIO 115 or BIO 161. 3 lectures, 1 laboratory. Fulfills GE B2 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '112', 'Environmental Biology and Conservation', 'Lec', '1', 'A biologically centered exploration of our planet focusing on natural resource conservation and contemporary environmental issues. Interactions between components of the biosphere and impacts of human society on interrelationships within ecosystems. Trends in natural resource conservation and biodiversity preservation. 4 lectures. Fulfills GE B5.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '114', 'Plant Diversity and Ecology', 'Lec', '1', 'Plant diversity and ecology in aquatic and terrestrial plant communities including adaptations of plants to their environment. Identification of common, local native plants and plant communities, uses of native plants by Native Americans, and human impacts on native plant communities. 2 lectures, 2 laboratories. Fulfills GE B2 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '114', 'Plant Diversity and Ecology', 'Lab', '4', 'Plant diversity and ecology in aquatic and terrestrial plant communities including adaptations of plants to their environment. Identification of common, local native plants and plant communities, uses of native plants by Native Americans, and human impacts on native plant communities. 2 lectures, 2 laboratories. Fulfills GE B2 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '123', 'Biology of Sex', 'Lec', '1', 'Fundamental principles of biology related to sexual reproduction: genetics, physiology, behavior, ecology and evolution of sex in a broad range of organisms. 4 lectures. Not open for major credit in Biological Sciences or Microbiology. Fulfills GE B2.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '160', 'Diversity and History of Life', 'Lec', '1', 'Overview of the history, diversity and genetic relatedness of life on Earth; broad-scale evolutionary framework of the organization and expansion of life on Earth. 3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '160', 'Diversity and History of Life', 'Lab', '4', 'Overview of the history, diversity and genetic relatedness of life on Earth; broad-scale evolutionary framework of the organization and expansion of life on Earth. 3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '161', 'Introduction to Cell and Molecular Biology', 'Lec', '3', 'Fundamentals of cellular biology with an emphasis on the molecular perspective of life:  metabolism, photosynthesis, cell structure and reproduction, meiosis, immunology, classical and molecular genetics, gene regulation.  3 lectures, 1 laboratory.  Fulfills GE B2 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '161', 'Introduction to Cell and Molecular Biology', 'Lab', '17', 'Fundamentals of cellular biology with an emphasis on the molecular perspective of life:  metabolism, photosynthesis, cell structure and reproduction, meiosis, immunology, classical and molecular genetics, gene regulation.  3 lectures, 1 laboratory.  Fulfills GE B2 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '162', 'Introduction to Organismal Form and Function', 'Lec', '1', 'Fundamentals of the structure and physiology of tissues and organs of plants and animals:  energy acquisition and food distribution, gas exchange and fluid transport, and sensing and responding to the environment.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '162', 'Introduction to Organismal Form and Function', 'Lab', '7', 'Fundamentals of the structure and physiology of tissues and organs of plants and animals:  energy acquisition and food distribution, gas exchange and fluid transport, and sensing and responding to the environment.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '200', 'Special Problems for Undergraduates', 'Ind', '3', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 4 units, with a maximum of 2 units per quarter.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '202', 'Orientation to Biotechnology', 'Lec', '1', 'Introduction to the diversity of fields in biotechnology.  Applications in agriculture, nutrition, medicine and environmental problems.  1 lecture, 1 activity.  Crosslisted as BIO/CHEM 202.  Formerly SCM 201.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '202', 'Orientation to Biotechnology', 'Act', '1', 'Introduction to the diversity of fields in biotechnology.  Applications in agriculture, nutrition, medicine and environmental problems.  1 lecture, 1 activity.  Crosslisted as BIO/CHEM 202.  Formerly SCM 201.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '211', 'Biology of Plants and Animals', 'Lec', '2', 'Plant and animal anatomy, physiology, diversity and life cycles.  How plants and animals acquire nutrients, reproduce, and adapt to environments.  Emphasis on hands-on activities and model organisms suited for the elementary classroom.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '211', 'Biology of Plants and Animals', 'Lab', '2', 'Plant and animal anatomy, physiology, diversity and life cycles.  How plants and animals acquire nutrients, reproduce, and adapt to environments.  Emphasis on hands-on activities and model organisms suited for the elementary classroom.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '213', 'Life Science for Engineers', 'Lec', '2', 'Fundamentals of life sciences:  energetics, cell biology, molecular and classical genetics, microbiology, organismal biology, and ecology.  2 lectures.  Fulfills GE B2.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '227', 'Wildlife Conservation Biology', 'Lec', '1', 'Historical development of wildlife biology and philosophies. Basic principles of ecology and evolution. Practices applied to wildlife. Current problems involving people-wildlife interactions with special reference to world biodiversity. 4 lectures. Fulfills GE B2.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '231', 'Human Anatomy and Physiology I', 'Lec', '1', 'Structure and function of the skeletal, muscular, nervous, endocrine, and integumentary systems.  Molecular, cellular, and organ system levels of organization.  Lab includes study of prosected human cadavers.  Not open for major credit to Biological Sciences majors.  Not open to students with credit in BIO 432 or ZOO 331.  4 lectures, 1 laboratory.  Formerly ZOO 231.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '231', 'Human Anatomy and Physiology I', 'Lab', '5', 'Structure and function of the skeletal, muscular, nervous, endocrine, and integumentary systems.  Molecular, cellular, and organ system levels of organization.  Lab includes study of prosected human cadavers.  Not open for major credit to Biological Sciences majors.  Not open to students with credit in BIO 432 or ZOO 331.  4 lectures, 1 laboratory.  Formerly ZOO 231.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '232', 'Human Anatomy and Physiology II', 'Lec', '1', 'Structure and function of the circulatory, immune, respiratory, digestive, urinary, and reproductive systems.  Molecular, cellular, and organ system levels of organization.  Lab includes study of prosected human cadavers.  Not open for major credit to Biological Sciences majors.  Not open to students with credit in BIO 433 or ZOO 332.  4 lectures, 1 laboratory.  Formerly ZOO 232.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '232', 'Human Anatomy and Physiology II', 'Lab', '4', 'Structure and function of the circulatory, immune, respiratory, digestive, urinary, and reproductive systems.  Molecular, cellular, and organ system levels of organization.  Lab includes study of prosected human cadavers.  Not open for major credit to Biological Sciences majors.  Not open to students with credit in BIO 433 or ZOO 332.  4 lectures, 1 laboratory.  Formerly ZOO 232.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '253', 'Orientation  to Health Professions', 'Act', '1', 'Observation in a healthcare setting.  Students will shadow healthcare practitioners on campus or in a community setting.  Specific placement depend on practitioner availability.  Total credit limited to 2 units with a maximum of 1 unit per quarter.  Credit/No Credit grading only.  1 activity.  Priority to BIO and MCRO majors.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '302', 'Human Genetics', 'Lec', '1', 'Basic principles of human inheritance, including the transmission of genetic traits, chromosomal abnormalities and their effects, gene structure and function, mutations and mutagenic agents, cancer genetics, population genetics, and principles of genetic counseling.  Not open for major credit in Biological Sciences or Microbiology.  Not open to students with credit in BIO 303 or BIO 351.  4 lectures.  Fulfills GE B5.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '303', 'Survey of Genetics', 'Lec', '1', 'Principles of heredity and variation.  Transmission genetics; molecular mechanisms of inheritance and gene expression.  Not open for major credit in Biological Sciences or Microbiology.  Not open to students with credit in BIO 351.  Credit will be granted in only one of the following courses:  BIO 302 or BIO 303.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '330', 'Extended Field Biology Activity', 'Act', '2', 'Minimum of two days of field instruction in places with significant biological diversity, and an individual or group project.  Focus on field notebooks, field identification, survey methods, experimental design, and significant habitat types for various groups of organisms.  The Schedule of Classes will list the title of the associated field biology course.  Total credit limited to 6 units, each associated with a different field biology course, with no more than 4 units applied as advisor approved electives.  Field trip required.  1-3 activities.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '351', 'Principles of Genetics', 'Lec', '2', 'Principles of genetics and genetic analysis, including underlying molecular mechanisms.  Subjects include gene structure and function, inheritance patterns, regulation of gene expression, mutation, recombination, recombinant DNA technology, and an introduction to population genetics.  5 lectures.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '361', 'Principles of Physiology', 'Lec', '1', 'Fundamental principles of animal physiology, including cellular mechanisms and integration to whole animals.  Membrane transport, fluid/salt balance, excitable cells, metabolic rate, temperature, gas exchange and circulation.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '361', 'Principles of Physiology', 'Lab', '3', 'Fundamental principles of animal physiology, including cellular mechanisms and integration to whole animals.  Membrane transport, fluid/salt balance, excitable cells, metabolic rate, temperature, gas exchange and circulation.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '30', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '401', 'Principles of Conservation Biology', 'Lec', '1', 'Foundational concepts in the conservation of wild organisms and their habitats.  Quantification and valuation of biological diversity, current threats to diversity, and approaches to better understand and address these threats, across terrestrial, freshwater, and marine environments.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '406', 'Neuroscience', 'Lec', '1', 'Anatomy and physiology of nervous systems including electrophysiology, molecular and cellular mechanisms of neurotransmission, interactions between the nervous system and other body systems, and comparative anatomy of vertebrate nervous systems.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '406', 'Neuroscience', 'Lab', '1', 'Anatomy and physiology of nervous systems including electrophysiology, molecular and cellular mechanisms of neurotransmission, interactions between the nervous system and other body systems, and comparative anatomy of vertebrate nervous systems.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '408', 'Cardiorespiratory Physiology', 'Lec', '1', 'Anatomy and physiology of the cardiovascular, respiratory, and renal systems, with an emphasis on humans and other vertebrates.  Discussion of health and disease states and responses to exercise and environmental factors.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '408', 'Cardiorespiratory Physiology', 'Lab', '2', 'Anatomy and physiology of the cardiovascular, respiratory, and renal systems, with an emphasis on humans and other vertebrates.  Discussion of health and disease states and responses to exercise and environmental factors.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '409', 'Muscle and Locomotion', 'Lec', '1', 'Anatomy and physiology of musculoskeletal systems, including energetics and biomechanics of locomotion.  Discussion of invertebrates and vertebrates including humans.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '409', 'Muscle and Locomotion', 'Lab', '2', 'Anatomy and physiology of musculoskeletal systems, including energetics and biomechanics of locomotion.  Discussion of invertebrates and vertebrates including humans.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '414', 'Evolution', 'Lec', '2', 'Scientific evaluation of the theories, mechanism, and patterns of biological evolution.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '426', 'Immunology', 'Lec', '1', 'Principles of molecular and cellular immunology.  Emphasis on molecular regulation of immune cell development, including generation of unique receptors, lymphocyte signal transduction and selection, programmed cell death and regulation of immune responses.  Discussion and demonstration of roles of immunology in disease and as diagnostic tools.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '426', 'Immunology', 'Lab', '2', 'Principles of molecular and cellular immunology.  Emphasis on molecular regulation of immune cell development, including generation of unique receptors, lymphocyte signal transduction and selection, programmed cell death and regulation of immune responses.  Discussion and demonstration of roles of immunology in disease and as diagnostic tools.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '427', 'Wildlife Management', 'Lec', '1', 'Important habitats, such as riparian, wetlands, and habitat features important to wildlife, such as vegetation types and snags.  Basic concepts of wildlife management.  Emphasis on planning and designing habitats to meet the needs of wildlife.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '427', 'Wildlife Management', 'Lab', '2', 'Important habitats, such as riparian, wetlands, and habitat features important to wildlife, such as vegetation types and snags.  Basic concepts of wildlife management.  Emphasis on planning and designing habitats to meet the needs of wildlife.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '435', 'Plant Physiology', 'Lec', '1', 'Consideration of the principal physiological and biochemical processes of plants with emphasis on water relations, mineral nutrition, photosynthesis, and the physiology of plant development.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '435', 'Plant Physiology', 'Lab', '1', 'Consideration of the principal physiological and biochemical processes of plants with emphasis on water relations, mineral nutrition, photosynthesis, and the physiology of plant development.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '445', 'Community Ecology', 'Lec', '1', 'Principles of ecology at the community level including the mechanism that structure ecological communities, and the quantitative methods used to study community ecology such as diversity metrics, community composition analyses, interaction strengths and the application of statistics to field and experimental studies.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '445', 'Community Ecology', 'Lab', '2', 'Principles of ecology at the community level including the mechanism that structure ecological communities, and the quantitative methods used to study community ecology such as diversity metrics, community composition analyses, interaction strengths and the application of statistics to field and experimental studies.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '450', 'Undergraduate Laboratory Assistantship', 'Ind', '1', 'Assisting the instructor in teaching and supervising undergraduate laboratories in the Biological Sciences Department.  Total credit limited to 8 units, with a maximum of 4 units per quarter.  Credit/No Credit grading only.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '452', 'Cell Biology', 'Lec', '1', 'Introduction to cell structure and function, energy conversions, protein sorting, signaling, cytoskeleton, cell adhesion, and the cell cycle.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '452', 'Cell Biology', 'Lab', '2', 'Introduction to cell structure and function, energy conversions, protein sorting, signaling, cytoskeleton, cell adhesion, and the cell cycle.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '461', 'Senior Project - Research Proposal', 'Act', '2', 'Completion of a research proposal and literature review, including analysis of experimental results from published peer-reviewed articles in biology.  Written and oral presentations.  2 activities.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '462', 'Senior Project - Research', 'Ind', '4', 'Completion of a research project or equivalent in the biological sciences, selected and conducted in consultation with an instructor.  Results are presented in written reports.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '463', 'Honors Research', 'Ind', '1', 'Advanced laboratory research.  Results are presented in a poster session or other public forum.  Total credit limited to 2 units with a maximum of 1 unit per quarter.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '475', 'Molecular Biology Laboratory', 'Lec', '1', 'Introduction to techniques used in molecular biology and biotechnology; DNA extraction, characterization, cloning, Southern blotting, reverse transcription, polymerase chain reaction, and sequencing analysis.  1 lecture, 2 laboratories.  Crosslisted as BIO/CHEM 475.  Formerly BIO/CHEM 375.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '475', 'Molecular Biology Laboratory', 'Lab', '2', 'Introduction to techniques used in molecular biology and biotechnology; DNA extraction, characterization, cloning, Southern blotting, reverse transcription, polymerase chain reaction, and sequencing analysis.  1 lecture, 2 laboratories.  Crosslisted as BIO/CHEM 475.  Formerly BIO/CHEM 375.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '485', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '495', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '500', 'Individual Study', 'Ind', '3', 'Advanced independent study planned and completed under the direction of a member of the department faculty.  Total credit limited to 6 units.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '502', 'Biology of Organisms', 'Lec', '1', 'Principles of and current topics in organismal biology, with an emphasis on physiology (including organ systems), behavior, and responses to the environment.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '502', 'Biology of Organisms', 'Lab', '1', 'Principles of and current topics in organismal biology, with an emphasis on physiology (including organ systems), behavior, and responses to the environment.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '537', 'Advanced Behavioral Ecology', 'Act', '1', 'Function and evolution of behavioral traits as they relate to ecological phenomena.  Habitat selection, migration, spacing mechanisms, reproductive strategies, feeding strategies, agonistic, parasitic, altruistic behavior, communication, and comparative social systems.  1 activity.  Formerly ZOO 537.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '561', 'Proposal Writing for Biological Research', 'Sem', '1', 'Written and oral presentations of a proposal for research in biology including a literature review.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '575', 'College Biology Teaching Practicum', 'Act', '1', 'Part-time teaching assignment in an undergraduate college classroom.  Includes teaching and related activities under the supervision of a professor in Biological Science.  Total credit limited to 2 units.  Credit/No Credit grading only.  1-2 activities.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '585', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '590', 'Seminar in Biology', 'Act', '3', 'Problems and topics in advanced biology selected according to the interest and needs of the students enrolled.  Total credit limited to 6 units.  1 activity.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '591', 'Trends in Biology', 'Act', '1', 'Recent trends in the field of biology for graduate students in the Biological Sciences masters degree program.  Overview of current research with presentations from visiting scholars and Cal Poly faculty.  Total credit limited to 3 units.  1 activity.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '593', 'Regenerative Medicine Internship', 'Ind', '1', 'Supervised graduate research and/or development in stem cell science or regenerative medicine and engineering.  Provides students with an off-campus industrial or university internship.  Total credit limited to 10 units.  Crosslisted as ASCI/BIO/BMED 593.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '595', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', '599', 'Thesis', 'Ind', '4', 'Individual research in soil science under faculty supervision, leading to a scholarly written presentation exhibiting originality, clarity, critical and independent thinking, proper analysis of data, appropriate organization and format, and accurate and thorough documentation.  Six units required for the M.S.  degree.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', 'P351', 'Principles of Genetics', 'Lec', '1', 'Principles of genetics and genetic analysis, including underlying molecular mechanisms.  Subjects include gene structure and function, inheritance patterns, regulation of gene expression, mutation, recombination, recombinant DNA technology, and an introduction to population genetics.  5 lectures.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', 'P452', 'Cell Biology', 'Lec', '1', 'Introduction to cell structure and function, energy conversions, protein sorting, signaling, cytoskeleton, cell adhesion, and the cell cycle.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BIO', 'P452', 'Cell Biology', 'Lab', '1', 'Introduction to cell structure and function, energy conversions, protein sorting, signaling, cytoskeleton, cell adhesion, and the cell cycle.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '102', 'Introduction to Biomedical Engineering Analysis', 'Sem', '1', 'General introduction to bioengineering analysis applied to representative topics in biomechanics, biofluidics, bioinstrumentation, biomaterials, biotechnology, and related areas.  Review of technological needs, testing procedures, governmental regulation, quality of life, and ethical issues.  1 seminar.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '212', 'Introduction to Biomedical Engineering Design', 'Lec', '1', 'General introduction to bioengineering design, including examples of engineering analysis and design applied to representative topics in biomechanics, bioinstrumentation, biomaterials, biotechnology, and related areas.  A review of technological needs, design methodology, testing procedures, statistical analysis, governmental regulations, evaluation of costs and benefits, quality of life, and ethical issues.  2 lectures, 1 laboratory.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '212', 'Introduction to Biomedical Engineering Design', 'Lab', '2', 'General introduction to bioengineering design, including examples of engineering analysis and design applied to representative topics in biomechanics, bioinstrumentation, biomaterials, biotechnology, and related areas.  A review of technological needs, design methodology, testing procedures, statistical analysis, governmental regulations, evaluation of costs and benefits, quality of life, and ethical issues.  2 lectures, 1 laboratory.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '213', 'Bioengineering Fundamentals', 'Lec', '2', 'Treatment of the engineering applications of biology.  Genetic engineering and the industrial application of microbiology.  Systems physiology with engineering applications.  Structure and function relationships in biological systems.  The impact of life on its environment.  2 lectures.  Crosslisted as BRAE/BMED 213.  Fulfills GE B2.  Formerly BRAE/ENGR 213.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '310', 'Biomedical Engineering Measurement and Analysis', 'Lec', '1', 'Fundamentals of biomedical engineering analysis.  Use and application of tools and analytical methods used by bioengineers.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '310', 'Biomedical Engineering Measurement and Analysis', 'Lab', '2', 'Fundamentals of biomedical engineering analysis.  Use and application of tools and analytical methods used by bioengineers.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '355', 'Electrical Engineering Concepts for Biomedical Engineering', 'Lec', '1', 'Introduction to electrical engineering concepts for biomedical engineers.  Continuation of basic circuit analysis.  Steady state AC circuit analysis and phasor concepts.  Application of the Laplace Transform to transient circuit analysis.  Introduction to digital logic gates, combinational and sequential logic circuits.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '12', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '404', 'Applied Finite Element Analysis', 'Lec', '2', 'Finite element based solutions to engineering problems with an emphasis on elastostatic problems in structural mechanics.  The power and pitfalls associated with the finite element method highlighted through practical modeling assignments.  Introduces the use of commercial finite element codes.  3 lectures, 1 laboratory.  Crosslisted as BMED/CE/ME 404.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '404', 'Applied Finite Element Analysis', 'Lab', '3', 'Finite element based solutions to engineering problems with an emphasis on elastostatic problems in structural mechanics.  The power and pitfalls associated with the finite element method highlighted through practical modeling assignments.  Introduces the use of commercial finite element codes.  3 lectures, 1 laboratory.  Crosslisted as BMED/CE/ME 404.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '410', 'Biomechanics', 'Lec', '1', 'Fundamental biomechanical concepts and their application to human systems including examination of activities of daily living and performance settings.  Examination of the effects of chronic and acute loading on the body.  3 lectures, 1 laboratory.  Formerly KINE 302.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '410', 'Biomechanics', 'Lab', '2', 'Fundamental biomechanical concepts and their application to human systems including examination of activities of daily living and performance settings.  Examination of the effects of chronic and acute loading on the body.  3 lectures, 1 laboratory.  Formerly KINE 302.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '420', 'Principles of Biomaterials Design', 'Lec', '1', 'Fundamentals of materials science as applied to bioengineering design.  Biocompatibility of materials.  Materials characterization and design.  Natural and synthetic polymeric materials.  Wound repair, foreign body response, blood clotting.  Transplantation biology, artificial organs, and tissue engineering.  Medical devices, government regulations, and ethical issues.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '420', 'Principles of Biomaterials Design', 'Lab', '3', 'Fundamentals of materials science as applied to bioengineering design.  Biocompatibility of materials.  Materials characterization and design.  Natural and synthetic polymeric materials.  Wound repair, foreign body response, blood clotting.  Transplantation biology, artificial organs, and tissue engineering.  Medical devices, government regulations, and ethical issues.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '430', 'Biomedical Modeling and Simulation', 'Lec', '1', 'Computational methods for anatomical modeling and boundary value problems in the biomechanics of tissues and biomedical devices.  Nonlinear biodynamics, heat flow, cardiac impulse propagation, anatomic modeling, and biomechanics.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '430', 'Biomedical Modeling and Simulation', 'Lab', '2', 'Computational methods for anatomical modeling and boundary value problems in the biomechanics of tissues and biomedical devices.  Nonlinear biodynamics, heat flow, cardiac impulse propagation, anatomic modeling, and biomechanics.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '434', 'Micro/Nano Fabrication', 'Lec', '1', 'Fabrication science and technology for creating micro and nano scale devices.  Explore basic processes such as oxidation, diffusion, ion implantation, etching, chemical and physical vapor deposition, photolithography.  Develop an understanding of the science of each process and how to select the right steps for fabricating electronic, photon and micro-electro-mechanical systems devices.  3 lectures.  Crosslisted as BMED 434/EE 423/MATE 430.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '435', 'Microfabrication Laboratory', 'Lab', '2', 'Application of basic processes involved in microfabrication:  cleanroom protocol, oxidation, diffusion, photolithography etching and sputtering.  Explore process development through fabrication of electronic, photonic or microfluidic devices.  Each student will be part of a team that will fabricate and test a device.  1 laboratory.  Crosslisted as BMED/MATE 435.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '440', 'Bioelectronics and Instrumentation', 'Lec', '1', 'Analog and digital circuits in bioinstrumentation.  Biomedical signals in continuous and discrete systems.  Sampling and digital signal processing.  Ultrasound, MRI, CT, Bioelectromagnetics.  Electrokinetics.  Biophysical phenomena, transducers, and electronics as related to the design of biomedical instrumentation.  Potentiometric and amperometric signals and amplifiers.  Biopotentials, membrane potentials, chemical sensors.  Mechanical transducers for displacement, force and pressure.  Temperature sensors.  Flow sensors.  Light-based instrumentation.  Electrical safety.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '440', 'Bioelectronics and Instrumentation', 'Lab', '2', 'Analog and digital circuits in bioinstrumentation.  Biomedical signals in continuous and discrete systems.  Sampling and digital signal processing.  Ultrasound, MRI, CT, Bioelectromagnetics.  Electrokinetics.  Biophysical phenomena, transducers, and electronics as related to the design of biomedical instrumentation.  Potentiometric and amperometric signals and amplifiers.  Biopotentials, membrane potentials, chemical sensors.  Mechanical transducers for displacement, force and pressure.  Temperature sensors.  Flow sensors.  Light-based instrumentation.  Electrical safety.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '450', 'Contemporary Issues in Biomedical Engineering', 'Lec', '4', 'Current and evolving topics in biomedical engineering, including medical and industrial applications.  Exploration of contemporary issues in biomedical engineering, including technical and societal implications.  The Schedule of Classes will list topic selected.  Total credit limited to 16 units.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '455', 'Biomedical Engineering Design I', 'Lec', '1', 'Engineering design methodology, design process, project planning, decision making, modeling, construction, and testing of an open-ended design project.  Preparation of formal engineering reports.  Statistical analysis.  Governmental regulations.  Bioethical issues.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '455', 'Biomedical Engineering Design I', 'Lab', '2', 'Engineering design methodology, design process, project planning, decision making, modeling, construction, and testing of an open-ended design project.  Preparation of formal engineering reports.  Statistical analysis.  Governmental regulations.  Bioethical issues.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '456', 'Biomedical Engineering Design II: Senior Project', 'Lec', '1', 'Engineering design methodology, design process, project planning, decision making, modeling, construction, and testing of an open-ended design project.  Preparation of formal engineering reports.  Statistical analysis.  Governmental regulations.  Bioethical issues.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '456', 'Biomedical Engineering Design II: Senior Project', 'Lab', '2', 'Engineering design methodology, design process, project planning, decision making, modeling, construction, and testing of an open-ended design project.  Preparation of formal engineering reports.  Statistical analysis.  Governmental regulations.  Bioethical issues.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '459', 'Senior Thesis', 'Ind', '12', 'Selection and completion of senior thesis under faculty supervision.  Projects typical of problems which graduates must solve in their fields of employment.  Thesis results presented in a formal report.  Minimum commitment of 120 hours.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '500', 'Individual Study', 'Ind', '12', 'Advanced independent study planned and completed under the direction of a member of the department faculty.  Total credit limited to 6 units.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '520', 'Introduction to Biomedical Engineering', 'Lec', '1', 'Advanced treatment of the basic engineering sciences in the biomedical engineering context.  For the student who has had little prior exposure to biomedical engineering, but has either a strong engineering or a strong science background.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '530', 'Biomaterials', 'Lec', '1', 'Structure-function relationships for materials in contact with biological systems.  Interactions of materials implanted in the body.  Histological and hematological considerations including foreign body responses, inflammation, carcinogenicity, thrombosis, hemolysis, immunogenic and toxic properties.  Microbial interaction with material surfaces, degradation.  4 lectures.  Crosslisted as BMED/MATE 530.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '550', 'Current and Evolving Topics in Biomedical Engineering', 'Lec', '1', 'Current topics in biomedical engineering, including medical and industrial applications.  Exploration of detailed technical treatment of contemporary issues in biomedical engineering, and examination of technical and societal implications of these subjects.  The Schedule of Classes will list topics selected.  Total credit limited to 8 units.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '560', 'Cell Transplantation and Biotherapeutics', 'Lec', '1', 'Topics include the etiology, patho-physiology, and rodent models for various forms of disease, such as inflammatory, autoimmune, and monogenic diseases, as well as nucleic acid, protein, and cellular-based therapies for these conditions.  Not open to students with credit in BMED 545.  2 lectures.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '561', 'Cell Transplantation and Biotherapeutics Laboratory', 'Lab', '1', 'Procedures include rodent handling, anesthesia, surgically modeling disease, biotherapy delivery, and visualizing/measuring therapeutic efficacy.  Additional focus on experimental design, data collection, and analysis.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '593', 'Regenerative Medicine Internship', 'Ind', '1', 'Supervised graduate research and/or development in stem cell science or regenerative medicine and engineering.  Provides students with an off-campus industrial or university internship.  Total credit limited to 10 units.  Crosslisted as ASCI/BIO/BMED 593.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', '599', 'Design Project (Thesis)', 'Ind', '12', 'Each individual or group will be assigned a project for solution under faculty supervision as a requirement for the masters degree, culminating in a written report/thesis.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', 'P102', 'Introduction to Biomedical Engineering Analysis', 'Sem', '1', 'General introduction to bioengineering analysis applied to representative topics in biomechanics, biofluidics, bioinstrumentation, biomaterials, biotechnology, and related areas.  Review of technological needs, testing procedures, governmental regulation, quality of life, and ethical issues.  1 seminar.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BMED', 'P599', 'Design Project (Thesis)', 'Ind', '1', 'Each individual or group will be assigned a project for solution under faculty supervision as a requirement for the masters degree, culminating in a written report/thesis.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BOT', '121', 'General Botany', 'Lec', '1', 'The anatomy, physiology, reproduction, and importance of plants. 3 lectures, 1 laboratory. Fulfills GE B2 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BOT', '121', 'General Botany', 'Lab', '5', 'The anatomy, physiology, reproduction, and importance of plants. 3 lectures, 1 laboratory. Fulfills GE B2 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BOT', '313', 'Taxonomy of Vascular Plants', 'Lec', '1', 'Introduction to classification and identification of vascular plants, emphasizing major plant families; field and herbarium techniques.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BOT', '313', 'Taxonomy of Vascular Plants', 'Lab', '3', 'Introduction to classification and identification of vascular plants, emphasizing major plant families; field and herbarium techniques.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '121', 'Agricultural Mechanics', 'Lec', '1', 'Identification and use of tools and materials; shop safety; tool sharpening and care; concrete mixes and materials; simple electric wiring; metal work; pipe fitting; basic woodworking; estimating quantities and costs. Students are required to meet safety regulations in laboratory work. 1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '121', 'Agricultural Mechanics', 'Lab', '2', 'Identification and use of tools and materials; shop safety; tool sharpening and care; concrete mixes and materials; simple electric wiring; metal work; pipe fitting; basic woodworking; estimating quantities and costs. Students are required to meet safety regulations in laboratory work. 1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '129', 'Laboratory Skills and Safety', 'Lab', '1', 'Introduction to fabrication and construction materials used in the field of Agricultural Engineering.  Fabrication skills in the development of wood, metal, concrete projects, and creative design.  Strength tests of wood, fasteners, concrete, and student design projects.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '133', 'Introduction to Engineering Design Graphics', 'Lab', '2', 'Visual communication in engineering design and problem solving. Principles of freehand sketching, engineering graphics, and computer-aided-drafting. Perspective and orthographic sketching, orthographic drawing with instruments and computer, applied descriptive geometry. 1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '151', 'CAD for Agricultural Engineering', 'Lab', '2', 'Computer aided drafting on a desktop personal computer using Autocad software.  Drawing setup.  2-D projections including automatic dimensioning and hatching.  Isometric construction, drawing layers, library symbols.  Use of 3-D drawing software.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '152', '3-D Solids Modeling', 'Lab', '1', 'Introduction to 3-dimensional solids modeling using state-of-the-art software.  Model generation and modification of associative properties, assembly modeling, extrusions and revolutions.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '200', 'Special Problems for Undergraduates', 'Ind', '16', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 4 units, with a maximum of 2 units per quarter.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '203', 'Agricultural Systems Analysis', 'Lec', '1', 'Agricultural Systems Analysis investigates the interrelationships between sub-components in an overall system.  Problem solving algorithms, network analysis, project planning techniques, and optimization.  2 lectures, 2 activities.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '203', 'Agricultural Systems Analysis', 'Act', '1', 'Agricultural Systems Analysis investigates the interrelationships between sub-components in an overall system.  Problem solving algorithms, network analysis, project planning techniques, and optimization.  2 lectures, 2 activities.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '213', 'Bioengineering Fundamentals', 'Lec', '2', 'Treatment of the engineering applications of biology.  Genetic engineering and the industrial application of microbiology.  Systems physiology with engineering applications.  Structure and function relationships in biological systems.  The impact of life on its environment.  2 lectures.  Crosslisted as BRAE/BMED 213.  Fulfills GE B2.  Formerly BRAE/ENGR 213.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '237', 'Introduction to Engineering Surveying', 'Lec', '1', 'An introduction to basic field note keeping as well as the use of steel tapes, automatic levels, total stations and survey tools.  Training in the procedures for differential and profile leveling; angle measurement and traversing.  Hands-on experience with the use of GPS for surveying.  An understanding in computations to determine direction, elevations, and earthwork volumes.  Practice in map reading and building layout.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '237', 'Introduction to Engineering Surveying', 'Lab', '1', 'An introduction to basic field note keeping as well as the use of steel tapes, automatic levels, total stations and survey tools.  Training in the procedures for differential and profile leveling; angle measurement and traversing.  Hands-on experience with the use of GPS for surveying.  An understanding in computations to determine direction, elevations, and earthwork volumes.  Practice in map reading and building layout.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '239', 'Engineering Surveying', 'Lec', '3', 'Development of proper field note taking and procedures for measuring using automatic levels, total stations and GPS systems.  Understanding in the procedures and computations for differential leveling, profiles, traversing, triangulation and topographic surveys.  Computations in traverse adjustment, contour mapping, earthwork volumes, curve alignments and building layout.  Understanding in map reading, the use of datums, photogrammetry, CAD design and boundary law.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '239', 'Engineering Surveying', 'Lab', '3', 'Development of proper field note taking and procedures for measuring using automatic levels, total stations and GPS systems.  Understanding in the procedures and computations for differential leveling, profiles, traversing, triangulation and topographic surveys.  Computations in traverse adjustment, contour mapping, earthwork volumes, curve alignments and building layout.  Understanding in map reading, the use of datums, photogrammetry, CAD design and boundary law.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '240', 'Agricultural Engineering Laboratory', 'Lab', '1', 'Individual projects.  Total credit limited to 4 units.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '247', 'Forest Surveying', 'Lec', '1', 'Use and care of tapes, staff compass, abney levels, total stations, and GPS receivers.  Keeping field notes, measurements by tape.  Closed and open traverse by compass and total stations.  Turning angles and determining directions of lines.  Map reading and public land description.  GPS measurements.  Weekend field trips required.  1 lecture, 1 laboratory.  Crosslisted as BRAE/NR 247.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '247', 'Forest Surveying', 'Lab', '1', 'Use and care of tapes, staff compass, abney levels, total stations, and GPS receivers.  Keeping field notes, measurements by tape.  Closed and open traverse by compass and total stations.  Turning angles and determining directions of lines.  Map reading and public land description.  GPS measurements.  Weekend field trips required.  1 lecture, 1 laboratory.  Crosslisted as BRAE/NR 247.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '302', 'Servo Hydraulics', 'Lec', '1', 'Application of microcomputers and programmable logic controllers to hydraulic, pneumatic and mechanical systems.  Theory, instrumentation and sensors used in process and control systems used in agricultural equipment.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '302', 'Servo Hydraulics', 'Lab', '1', 'Application of microcomputers and programmable logic controllers to hydraulic, pneumatic and mechanical systems.  Theory, instrumentation and sensors used in process and control systems used in agricultural equipment.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '321', 'Agricultural Safety', 'Lec', '1', 'Principles of agricultural safety.  Accident causation and prevention, hazard identification and abatement, laws and regulations.  Machinery, electrical, chemical, livestock, shop and fire safety.  Safety program development.  2 lectures, 1 activity.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '321', 'Agricultural Safety', 'Act', '1', 'Principles of agricultural safety.  Accident causation and prevention, hazard identification and abatement, laws and regulations.  Machinery, electrical, chemical, livestock, shop and fire safety.  Safety program development.  2 lectures, 1 activity.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '324', 'Principles of Agricultural Electrification', 'Lec', '1', 'Applications of DC/AC electricity in agriculture.  National Electric Code regulations.  The wiring of agricultural structures and electrical distribution.  Series, parallel and series-parallel circuits, R-L-C circuits, electric motors, electronics.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '324', 'Principles of Agricultural Electrification', 'Lab', '1', 'Applications of DC/AC electricity in agriculture.  National Electric Code regulations.  The wiring of agricultural structures and electrical distribution.  Series, parallel and series-parallel circuits, R-L-C circuits, electric motors, electronics.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '331', 'Irrigation Theory', 'Lec', '1', 'Plant-water-soil relations using evapo-transpiration, plant stress, soil moisture deficiency, frequency and depth of irrigation, salinity, infiltration, drainage and climate control.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '335', 'Internal Combustion Engines', 'Lec', '1', 'Principles of operation of internal combustion engines.  Theory of operation and diagnosis evaluation and repair of small engines, gasoline and diesel engines and economics of operation, use and repair.  Power analysis and application.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '335', 'Internal Combustion Engines', 'Lab', '1', 'Principles of operation of internal combustion engines.  Theory of operation and diagnosis evaluation and repair of small engines, gasoline and diesel engines and economics of operation, use and repair.  Power analysis and application.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '337', 'Landscape Irrigation', 'Lec', '1', 'Design of sprinkler and drip irrigation systems including:  site characteristics, soil variables affecting water storage and infiltration rate, plant selection and hydrozones, hydraulics, nozzle spacing, selection of system components, back flow prevention, plumbing codes and cost estimating.  Irrigation system evaluation and audit irrigation scheduling, and water budget.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '337', 'Landscape Irrigation', 'Lab', '1', 'Design of sprinkler and drip irrigation systems including:  site characteristics, soil variables affecting water storage and infiltration rate, plant selection and hydrozones, hydraulics, nozzle spacing, selection of system components, back flow prevention, plumbing codes and cost estimating.  Irrigation system evaluation and audit irrigation scheduling, and water budget.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '339', 'Internship in Bioresource and Agricultural Engineering', 'Ind', '2', 'Students will spend up to 12 weeks with an approved agricultural firm engaged in production or related business.  Time will be spent applying and developing production and managerial skills and abilities.  One unit of credit may be allowed for each full week of completed and reported internship.  Degree credit limited to 6 units.  Credit/No Credit grading only.', '1-12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '340', 'Irrigation Water Management', 'Lec', '1', 'Soil-plant-water relationships; evapotranspiration; irrigation schedules; salinity and drainage; irrigation efficiency.  Water measurement; soil moisture measurement; irrigation systems and practical constraints affecting scheduling.  California water supply and budget; water rights; local, state and federal water institutions; California water issues.  3 lectures, 1 laboratory.  Fulfills GE Area F.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '340', 'Irrigation Water Management', 'Lab', '4', 'Soil-plant-water relationships; evapotranspiration; irrigation schedules; salinity and drainage; irrigation efficiency.  Water measurement; soil moisture measurement; irrigation systems and practical constraints affecting scheduling.  California water supply and budget; water rights; local, state and federal water institutions; California water issues.  3 lectures, 1 laboratory.  Fulfills GE Area F.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '343', 'Mechanical Systems Analysis', 'Lec', '1', 'Use of statics and dynamics to make original calculations, plans, sketches, graphics, drawings, schemes and layouts for the fabrication and construction of machines.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '343', 'Mechanical Systems Analysis', 'Lab', '1', 'Use of statics and dynamics to make original calculations, plans, sketches, graphics, drawings, schemes and layouts for the fabrication and construction of machines.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '345', 'Aerial Photogrammetry and Remote Sensing', 'Lec', '1', 'Object recognition, three-dimensional equipment, and interpretation of aerial photographs.  Print alignment, stereoscopic viewing, scales, elevation determination, and application.  Orthophotos and their relationship to Geographic Information Systems (GIS).  Application of aerial photos to regional studies.  2 lectures, 1 laboratory.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '345', 'Aerial Photogrammetry and Remote Sensing', 'Lab', '2', 'Object recognition, three-dimensional equipment, and interpretation of aerial photographs.  Print alignment, stereoscopic viewing, scales, elevation determination, and application.  Orthophotos and their relationship to Geographic Information Systems (GIS).  Application of aerial photos to regional studies.  2 lectures, 1 laboratory.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '348', 'Energy for a Sustainable Society', 'Lec', '1', 'Study of how the transition can be made from fossil fuels to renewable energy sources including hydro, biomass, solar, wind, and energy conservation.  Environmental, economic, and political consequences of a renewable energy-based sustainable society.  3 lectures, 1 activity.  Fulfills GE Area F.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '348', 'Energy for a Sustainable Society', 'Act', '2', 'Study of how the transition can be made from fossil fuels to renewable energy sources including hydro, biomass, solar, wind, and energy conservation.  Environmental, economic, and political consequences of a renewable energy-based sustainable society.  3 lectures, 1 activity.  Fulfills GE Area F.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '400', 'Special Problems', 'Ind', '16', 'Supervised investigation, including a written report, of a topic chosen with prior approval of instructor.  Total credit limited to 4 units.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '419', 'Agricultural Systems Management II', 'Lec', '1', 'Project management of agricultural systems.  Emphasis placed on a team approach to problem solution.  Case studies and student projects used to explore the following topics:  project leadership, project organization, communication, needs assessment, feasibility studies, cost analysis, decision making, solution implementation, and evaluation.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '419', 'Agricultural Systems Management II', 'Lab', '2', 'Project management of agricultural systems.  Emphasis placed on a team approach to problem solution.  Case studies and student projects used to explore the following topics:  project leadership, project organization, communication, needs assessment, feasibility studies, cost analysis, decision making, solution implementation, and evaluation.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '422', 'Equipment Engineering', 'Lec', '1', 'Design and construction of specialized agricultural components and equipment.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '422', 'Equipment Engineering', 'Lab', '2', 'Design and construction of specialized agricultural components and equipment.  2 lectures, 2 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '438', 'Drip/Micro Irrigation', 'Lec', '1', 'Drip/micro irrigation hardware and management.  Emphasizes agricultural drip/micro irrigation with some landscape application.  Filtration, emitters, chemical injection, agronomic constraints, and scheduling.  Field trip(s) included.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '438', 'Drip/Micro Irrigation', 'Lab', '1', 'Drip/micro irrigation hardware and management.  Emphasizes agricultural drip/micro irrigation with some landscape application.  Filtration, emitters, chemical injection, agronomic constraints, and scheduling.  Field trip(s) included.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '460', 'Senior Project Organization', 'Lec', '1', 'Selection and organization of senior project.  Involves time management, research techniques, budgeting and project presentation.  Documentation of multidisciplinary team experience.  1 lecture.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('BRAE', '461', 'Senior Project I', 'Ind', '14', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '1');
