INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('NR', '311', 'Environmental Measurements and Interpretation', 'Lec', '1', 'Measurement and interpretation of the biological, physical and social values of the natural elements of our environment; organization and presentation of interpretive materials by oral and written communication.  3 lectures, 1 laboratory.', '4');
