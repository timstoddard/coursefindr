INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '599', 'Thesis (Design Project)', 'Ind', '9', 'Each individual or group will be assigned a project for solution under faculty supervision as a requirement for the masters degree, culminating in a written report/thesis.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', 'P303', 'Aerothermodynamics III', 'Lec', '1', 'Properties and characteristics of fluids, fluid statics and dynamics, the thermodynamic relations, laminar and turbulent flows, subsonic and supersonic flows as applied to flight vehicles.  Introduction to heat transfer.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', 'P353', 'Spacecraft Environment', 'Lec', '1', 'Effects of the space environment on a spacecraft and design considerations.  Topics include the launch, vacuum, particulate, plasma, and radiation environments 4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', 'P433', 'Experimental Stress Analysis', 'Lab', '1', 'Employing the knowledge of stress analysis and aerospace structural analysis in an individual and group design project dealing with aerospace structures.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', 'P448', 'Spacecraft Design II', 'Lab', '1', 'Preliminary layout of typical space vehicle using design and calculation techniques developed in previous aerospace engineering courses.  Design of selected components and preparation of necessary drawings.  3 laboratories.  Open to students enrolled in the multidisciplinary design minor.', '3');
