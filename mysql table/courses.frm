INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AG', '595', 'Cooperative Education Experience', 'Ind', '5', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AG', '598', 'Reading and Conference', 'Ind', '13', 'Systematic development of an agricultural thesis research project including literature searches, reports and experimental design.  Repeatable for up to 12 units.  Credit/No Credit grading only.', '1-12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AG', '599', 'Thesis', 'Ind', '17', 'Individual research in soil science under faculty supervision, leading to a scholarly written presentation exhibiting originality, clarity, critical and independent thinking, proper analysis of data, appropriate organization and format, and accurate and thorough documentation.  Six units required for the M.S.  degree.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '202', 'Introduction to Sales', 'Lec', '3', 'Development of professional business-to-business selling principles within the supply chain, including an introduction to understanding the sales process from different buying and selling perspectives, communication techniques, and basic sales competency. 4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '212', 'Agricultural Economics', 'Lec', '4', 'Theoretical development of factors affecting demand and supply for food and fiber and for agricultural inputs. Methods of selecting optimal levels of agricultural production and consumption variables. Evaluation of market structure and price formulation for agricultural products and resources. 4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '214', 'Agribusiness Financial Accounting', 'Lec', '3', 'Principles of financial accounting in agribusiness. Preparation for understanding and interpreting financial statements. Exploration of financial reporting standards to provide an understanding of how financial events are reflected in financial statements. The importance of social responsibility in accounting. The accounting cycle, from transactions posting to financial statements through spreadsheet applications. 3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '214', 'Agribusiness Financial Accounting', 'Act', '6', 'Principles of financial accounting in agribusiness. Preparation for understanding and interpreting financial statements. Exploration of financial reporting standards to provide an understanding of how financial events are reflected in financial statements. The importance of social responsibility in accounting. The accounting cycle, from transactions posting to financial statements through spreadsheet applications. 3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '260', 'Agribusiness Data Literacy', 'Lec', '2', 'Using data and analysis in making decisions related to agribusiness.  Developing basic and intermediate spreadsheet skills necessary to organize, analyze, and summarize information.  Development of data management and analysis as tools to assist in agribusiness problem-solving.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '301', 'Food and Fiber Marketing', 'Lec', '2', 'Food and fiber marketing, examining commodity, industrial, and consumer product marketing from a managerial viewpoint.  A global perspective in understanding consumer needs and developing the knowledge of economic, political, social and environmental factors that affect food and fiber marketing systems.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '308', 'Introduction to Agribusiness Finance', 'Lec', '1', 'Concepts and analytical methods related to agricultural finance.  Focus on applied spreadsheet analysis of financial statements, time value of money, risk and return, portfolio theory, and capital budgeting within the context of financial institutions specific to agriculture.  Not open to students with credit in AGB 310.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '309', 'Advanced Sales Techniques', 'Lec', '2', 'Expansion of basic sales principles and skills, focusing on prospecting, sales styles, pipelining, forecasting, and product planning.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '310', 'Agribusiness Credit and Finance', 'Lec', '1', 'Financing Californias agricultural industry.  Sources of credit and types of loans used by agribusinesses.  Costs of credit.  Financial analysis of agricultural borrowers.  Future and present value techniques used in evaluating agricultural investments.  Agricultural financial management.  Financial capital markets and leasing.  Not open to Agricultural Business majors.  Not open to students with credit in AGB 308.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '311', 'Intermediate Agribusiness Finance', 'Lec', '1', 'Financial tools applied to the agribusiness industry, focusing on unique risks in agriculture.  Risk and return in the production and processing sectors, working capital management, cash flows and financial statement forecasting for agribusiness firms, using spreadsheet analysis.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '312', 'Agricultural Policy', 'Lec', '3', 'Agricultural policy objectives and formulation, resource allocation and production adjustments.  Survey of State and Federal agricultural policies as they influence the planning and practices of agribusiness.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '313', 'Agriculture Economic Analysis', 'Lec', '2', 'Advanced agricultural microeconomics with emphasis on mathematical problem solving; production and cost functions, single and multiple input allocation, agricultural output combinations, agricultural market structures, and economies of size.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '323', 'Agribusiness Managerial Accounting', 'Lec', '2', 'Agribusiness management with an emphasis on using accounting procedures that will provide useful information in making management decisions, setting objectives, and controlling operations.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '323', 'Agribusiness Managerial Accounting', 'Act', '4', 'Agribusiness management with an emphasis on using accounting procedures that will provide useful information in making management decisions, setting objectives, and controlling operations.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '327', 'Agribusiness Data Analysis', 'Lec', '1', 'Methods in agricultural business data analysis, including multiple regression analysis, analysis of variance, and time series analysis.  Applications include agricultural price forecasting and estimation of the determinants of food and fiber demand.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '327', 'Agribusiness Data Analysis', 'Act', '1', 'Methods in agricultural business data analysis, including multiple regression analysis, analysis of variance, and time series analysis.  Applications include agricultural price forecasting and estimation of the determinants of food and fiber demand.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '328', 'Decision Tools for Agribusiness', 'Lec', '2', 'Development of agribusiness modeling techniques that are applied to solving a diverse and unique set of resource allocation issues encountered throughout the agricultural and food retail sectors.  Techniques include linear programming, decision analysis, and computer simulations.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '328', 'Decision Tools for Agribusiness', 'Act', '2', 'Development of agribusiness modeling techniques that are applied to solving a diverse and unique set of resource allocation issues encountered throughout the agricultural and food retail sectors.  Techniques include linear programming, decision analysis, and computer simulations.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '339', 'Internship in Agribusiness', 'Ind', '1', 'Selected students will spend up to 12 weeks with an approved agricultural firm engaged in production or related agribusiness.  Time will be spent applying and developing agribusiness functional and managerial skills and abilities.  One unit of credit may be allowed for each full week of completed and reported internship.  Degree credit limited to 6 units.  Credit/No Credit grading only.', '1-12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '369', 'Agricultural Personnel Management', 'Lec', '1', 'Standard topics of California agricultural personnel management:  recruitment; appraisal and performance evaluation; compensation; training and development; discipline; safety and health; labor relations; and immigration policy.  Systemic approach to aspects of managing human capital, and how to implement human resource policies.  Not open to students with credit in AGB 401.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '400', 'Special Problems', 'Ind', '3', 'Supervised investigation, including a written report, of a topic chosen with prior approval of instructor.  Total credit limited to 4 units.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '406', 'Agribusiness Marketing Planning', 'Lec', '1', 'Development of agribusiness marketing plans in self-managed teams.  Emphasis on developing presentation skills.  Integration of marketing mix, particularly promotional elements in developing agribusiness marketing strategy emphasized.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '410', 'Agricultural Lending', 'Lec', '1', 'Structure and performance of the agricultural lending industry.  Advanced agricultural loan analysis and risk assessment.  Agricultural loan documentation, securitization of farm loans, and farm bankruptcy.  Exploration of interest rate impacts on agricultural lending.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '422', 'Transportation and Logistics in Global Agribusiness', 'Lec', '1', 'Scope and elements of the agribusiness logistics system including supply and distribution channels, transportation, inventory, warehousing, packaging, and order processing.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '425', 'Agribusiness Supply Chain Management', 'Lec', '1', 'Focus on the development and application of decision models in food supply chains with emphasis on demand forecasting, aggregate planning, inventory management (cycle and food safety), supply network design, transportation, coordination and sourcing.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '461', 'Senior Project', 'Ind', '1', 'Completion, under faculty supervision, of an investigative project typical of problems which graduates must solve in their fields of employment.  Required minimum of 90 hours.  Analytical, formal report is required.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '462', 'Senior Project - Applied Agribusiness Problems', 'Lec', '2', 'Selection and analysis of agribusiness problems and opportunities in directed group-based projects.  Exploration of problems which agribusiness graduate students may encounter in marketing, finance, management and production in the food and fiber industry.  Formal report and presentation required.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', '463', 'Senior Project - Agribusiness Consulting', 'Lec', '1', 'Client-centered course where self-managed teams develop solutions to agribusiness problems.  Exploration of problems typical to those which agribusiness graduates may encounter in marketing, finance, management and production in the food and fiber industry.  Formal report and presentation to client required.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGB', 'P202', 'Introduction to Sales', 'Lec', '1', 'Development of professional business-to-business selling principles within the supply chain, including an introduction to understanding the sales process from different buying and selling perspectives, communication techniques, and basic sales competency. 4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '102', 'Orientation to Agricultural Communication', 'Lec', '1', 'Orientation to the communication sector of agriculture. Overview of professional opportunities and skills needed for success in agricultural communications. Preparation of press releases and short articles, and development of a planned program of study. 2 lectures.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '200', 'Special Problems in Agricultural Communication', 'Ind', '4', 'Individual study of modern issues and problems conducted through research, planning and development.  Field problems and in-service study in agricultural industry encouraged.  Final written report required.  Total credit limited to 9 units with not more than 3 units in any one quarter.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '301', 'New Media Communication Strategies in Agriculture', 'Lec', '1', 'Exploration and implementation of emerging new media communication strategies and technologies to convey information on important issues in agriculture to a global audience.  Focus on food and farming dialogues currently populating conversations about production agriculture.  Adaptation of different writing styles based on requirements of the various new media channels.  Analysis of metrics to measure level of engagement with desired audience.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '301', 'New Media Communication Strategies in Agriculture', 'Lab', '1', 'Exploration and implementation of emerging new media communication strategies and technologies to convey information on important issues in agriculture to a global audience.  Focus on food and farming dialogues currently populating conversations about production agriculture.  Adaptation of different writing styles based on requirements of the various new media channels.  Analysis of metrics to measure level of engagement with desired audience.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '339', 'Internship in Agricultural Communications', 'Ind', '1', 'Selected Agricultural Communication students will spend up to 12 weeks with an approved agricultural firm engaged in production or related business.  Time will be spent applying and developing production and managerial skills and abilities.  One unit of credit may be allowed for each full week of completed and reported internship.  Degree credit limited to 6 units.  Credit/No Credit grading only.', '1-12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '400', 'Advanced Special Problems in Agricultural Communication', 'Ind', '3', 'Individual investigation, research, studies or surveys of selected problems in Agricultural Communication/Agricultural Education.  Total credit limited to 8 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '461', 'Senior Project I', 'Ind', '9', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '462', 'Senior Project II', 'Ind', '9', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '500', 'Individual Study In Agricultural Communication', 'Ind', '1', 'Advanced independent study planned and completed under the direction of a member of the Agricultural Education and Communication faculty.  Total credit limited to 6 units.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '539', 'Graduate Internship in Agricultural Communication', 'Ind', '1', 'Application of theory to the solution of problems of agricultural production or related business in the field of Agricultural Communication.  Analyze specific management problems and perform general management assignments detailed in a contract between the student, the firm or organization, and the faculty advisor before the internship commences.  Degree credit limited to 6 units.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGC', '580', 'Special Problems in Agricultural Communication', 'Ind', '1', 'Individual study of modern issues and problems conducted through research, planning and development.  Field problems and in-service study in agricultural industry encouraged.  Final written report required.  Total credit limited to 9 units with not more than 3 units in any one quarter.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '102', 'Introduction to Agricultural Education', 'Lec', '1', 'Overview of agricultural education career pathways including goals and purposes. Kinds of classes and types of programs. Qualifications essential to success in teaching agriculture. Planned program of studies to meet requirement for teaching agricultural science and related disciplines. 2 lectures.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '200', 'Special Problems in Agricultural Education', 'Ind', '4', 'Individual study of modern issues and problems conducted through research, planning and development.  Field problems and in-service study in agricultural industry encouraged.  Final written report required.  Total credit limited to 9 units with not more than 3 units in any one quarter.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '220', 'Agricultural Youth Conferences', 'Act', '1', 'Problems encountered and practices applied during the conduct of the annual FFA State Convention.  Methods, procedures and materials adapted for use by the student in developing the committee system to produce conferences, conventions and workshops of all kinds and sizes.  Total credit limited to 6 units.  Credit/No Credit grading only.  2 activities.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '339', 'Internship in Agricultural Education', 'Ind', '1', 'Selected Agricultural Education students will spend up to 12 weeks with an approved agricultural firm engaged in production or related business.  Time will be spent applying and developing production and managerial skills and abilities.  One unit of credit may be allowed for each full week of completed and reported internship.  Degree credit limited to 6 units.  Credit/No Credit grading only.', '1-12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '350', 'Early Field Experience in Agriculture Education', 'Sem', '1', 'Observation of the practices and techniques utilized by agriculture teachers in conducting organized instruction in high school classrooms, shops, school farms, and laboratories.  Discussion of activities and programs unique to teaching agriculture in California secondary schools.  Participation in public schools requires mandated fingerprint clearance.  2 seminars.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '400', 'Advanced Special Problems in Agricultural Education', 'Ind', '4', 'Individual investigation, research, studies or surveys of selected problems in Agricultural Communication/Agricultural Education.  Total credit limited to 8 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '404', 'Agricultural Leadership', 'Lec', '1', 'Emphasis upon equipping current and prospective leaders in agriculture with the background and skills to achieve their potential.  Class members will assess their status as leaders and identify means to improve their effectiveness.  Focus on the theoretical underpinnings of human motivation, personal leadership, and organizational development.  2 lectures, 1 activity.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '404', 'Agricultural Leadership', 'Act', '1', 'Emphasis upon equipping current and prospective leaders in agriculture with the background and skills to achieve their potential.  Class members will assess their status as leaders and identify means to improve their effectiveness.  Focus on the theoretical underpinnings of human motivation, personal leadership, and organizational development.  2 lectures, 1 activity.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '424', 'Organizing and Teaching Agriculture', 'Act', '1', 'Determining course objectives, content, and calendar for use by the teacher in classroom, shop and field instruction while assigned to community schools.  Concurrent with student teaching.  3 activities.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '440', 'Student Teaching in Agricultural Education', 'Ind', '7', 'Off-campus assignment to a selected cooperating public school. Participation in all phases of agriculture teacher duties and activities including departmental organization and administration. Prior approval and appointment necessary. Total credit limited to 18 units. Credit/No Credit grading only.', '6-12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '441', 'Student Teaching Practicum', 'Act', '1', 'Problems encountered and practices applied during student teaching.  Methods, procedures and materials adapted for use by the teacher concurrent with student teaching.  2 activities.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '460', 'Research Methodology in Agricultural Education and Communication', 'Lec', '1', 'Introduction to the research process and topic selection as it relates to the design and development of the senior project within the Agricultural Communication and the Agricultural Sciences majors.  1 lecture.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '461', 'Senior Project I', 'Ind', '9', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '462', 'Senior Project II', 'Ind', '9', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '500', 'Individual Study in Agricultural Education', 'Ind', '5', 'Advanced independent study planned and completed under the direction of a member of the Agricultural Education and Communication faculty.  Total credit limited to 6 units.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '513', 'Field Experience - Agricultural Education', 'Ind', '7', 'Practice and techniques in management and supervision of programs in agricultural education.  Relationships among students, staff, community and school groups.  Budgeting, staffing, records, reporting.  Student activities and Future Farmers of America programs.  Total credit limited to 6 units.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '536', 'Teaching Agriculture in Higher Education', 'Sem', '1', 'Selection and use of teaching strategies, methods/approaches, and techniques when planning, delivering, and evaluating instruction.  Intended for graduate students interested in pursuing a faculty position in agriculture, as well as other disciplines in higher education.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '539', 'Graduate Internship in Agricultural Education', 'Ind', '5', 'Application of theory to the solution of problems of agricultural production or related business in the field of Agricultural Education.  Analyze specific management problems and perform general management assignments detailed in a contract between the student, the firm or organization, and the faculty advisor before the internship commences.  Degree credit limited to 6 units.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AGED', '580', 'Special Problems in Agricultural Education', 'Ind', '5', 'Individual study of modern issues and problems conducted through research, planning and development.  Field problems and in-service study in agricultural industry encouraged.  Final written report required.  Total credit limited to 9 units with not more than 3 units in any one quarter.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '200', 'Special Problems for Undergraduates', 'Ind', '1', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 4 units, with a maximum of 2 units per quarter.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '201', 'Cultural Anthropology', 'Lec', '4', 'Contemporary human cultures throughout the world. General patterns sought within the diversity of individual cultures. Includes such topics as: family organization; gender roles; adaptation to the environment; systems of economic exchange; political organization and leadership; religious beliefs and values; ethnicity and cultural pluralism; impact of Western culture on the developing world. 4 lectures. Fulfills GE D3.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '250', 'Biological Anthropology', 'Lec', '1', 'Biological aspects of human unity and diversity. Primate and human evolution, including anatomical, physiological and behavioral adaptations. Origin and diversity of modern races. 4 lectures. Fulfills GE B2.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '309', 'Elements of Archaeology', 'Lec', '1', 'Archaeological method and theory covering the history and development of archaeological thought, approaches to data recovery, dating and analysis of artifacts and ecofacts, the construction of models of prehistoric human behavior through application of archaeological and anthropological theories.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '311', 'Archaeological Laboratory Methods', 'Lec', '1', 'Hands-on introduction to the methods employed in post-field processing, classification, analysis, and preservation of archaeological materials.  Compilation of quantitative and qualitative information in data base format to assist in the classification and interpretation of faunal remains and artifacts.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '311', 'Archaeological Laboratory Methods', 'Lab', '1', 'Hands-on introduction to the methods employed in post-field processing, classification, analysis, and preservation of archaeological materials.  Compilation of quantitative and qualitative information in data base format to assist in the classification and interpretation of faunal remains and artifacts.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '344', 'Sex, Death, and Human Nature', 'Lec', '2', 'How Darwinian processes of differential reproduction and mortality influence human interests, passions and behaviors.  Theories of inclusive fitness, parental investment and senescence.  Sex differences, sexual attraction, life histories, violence and aggression, including rape, homicide and infanticide.  4 lectures.  Fulfills GE D5 except for Anthropology/Geography and Social Sciences majors.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '360', 'Human Cultural Adaptations', 'Lec', '2', 'Social and cultural evolution from Paleolithic times to the present.  Interactions of demographic, economic and ecological factors are emphasized.  Main topics include human nature/culture, sex and gender, cooperation and conflict, the agricultural revolution, state formation, social inequality and globalization.  4 lectures.  Fulfills GE D5 except for Anthropology/Geography and Social Sciences majors.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '1', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '401', 'Culture and Health', 'Lec', '1', 'Global perspective on the relationship between culture and health.  Ecological factors influencing health and illness.  Origins of disease and impact of diseases on society.  Diet and nutrition.  Classifications of illness causation.  Kinds of curers.  Relationship of gender and reproduction to illness.  Pharmacology.  Mental illness.  Global health problems.  Alternative health care modalities.  Health-care needs of U.S.  ethnic groups.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '461', 'Senior Project I', 'Ind', '4', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '462', 'Senior Project II', 'Ind', '4', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', '465', 'Internship', 'Ind', '1', 'Part-time work experience in the entertainment industry.  Ability to work independently; strong verbal and written skills.  Faculty approval of job position required.  Evaluations by job supervisor and written reports by student required.  120 hours of work experience.  Total credit limited to 8 units.  Credit/No Credit grading.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ANT', 'P201', 'Cultural Anthropology', 'Lec', '1', 'Contemporary human cultures throughout the world. General patterns sought within the diversity of individual cultures. Includes such topics as: family organization; gender roles; adaptation to the environment; systems of economic exchange; political organization and leadership; religious beliefs and values; ethnicity and cultural pluralism; impact of Western culture on the developing world. 4 lectures. Fulfills GE D3.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '211', 'Structures I', 'Lec', '4', 'Introduction to the role of structures in the making of buildings.  Introduction to statics and creation of simple three-dimensional structures.  Development of skills to analyze structures composed of axial force (truss) members.  2 lectures, 1 activity.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '211', 'Structures I', 'Act', '4', 'Introduction to the role of structures in the making of buildings.  Introduction to statics and creation of simple three-dimensional structures.  Development of skills to analyze structures composed of axial force (truss) members.  2 lectures, 1 activity.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '212', 'Structures II', 'Lec', '9', 'Introduction to the role of structures in the making of buildings.  Introduction to shear and moment diagrams using the principles of statics and the application of the diagrams to simple three-dimensional structures.  Development of skills, particularly free body diagrams, to analyze structures composed of bending (beams) members.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '226', 'Introduction to Structural Systems', 'Lec', '1', 'Description, behavior and comparison of structural building systems.  Concepts of structural stability, load flow, framing schemes and building configuration related to vertical and lateral loads.  Not open to Architectural Engineering majors.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '257', 'Structural CAD for Building Design', 'Lec', '1', 'Emphasis on the use of computer graphics software to represent a buildings structural system and its individual elements.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '257', 'Structural CAD for Building Design', 'Lab', '3', 'Emphasis on the use of computer graphics software to represent a buildings structural system and its individual elements.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '302', 'Structural Analysis', 'Lec', '1', 'Structural analysis of frames, trusses, and combined systems.  Modern structural analysis theorems are presented along with discussion of their relation to classical methods.  Specific topics include virtual forces, virtual displacements, compatibility, constraints and matrix formulations.  Course may be offered in classroom-based or online format.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '303', 'Steel Design I', 'Lec', '3', 'Analysis and design of steel structural members subjected to bending, shear and axial forces.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '304', 'Timber Design', 'Lec', '1', 'Analysis and design of timber structural members subjected to bending, shear, and axial forces.  Wood diaphragms, shear walls and their connections.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '305', 'Masonry Design', 'Lec', '1', 'Design of load-bearing walls, shear walls, columns and beams in masonry.  2 lectures.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '306', 'Matrix Analysis of Structures', 'Lec', '2', 'Analysis of statically indeterminate structures by direct stiffness method including continuous beams, plane trusses, and frames.  Introduction to finite-element methods.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '315', 'Introduction to Structural Design', 'Lec', '4', 'Introduction to structures that use timber, steel and concrete as the primary construction material.  Introduction to gravity load carrying systems and lateral load resisting systems using timber, steel and concrete elements.  Credit not allowed for ARCE majors.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '352', 'Structural Computing Analysis', 'Lab', '2', 'Computer calculations, programming and technical reporting.  Emphasis on use of two-dimensional structural analysis software to analyze a buildings structural system and its individual elements.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '353', 'Matrix Structural Computing Analysis', 'Lab', '4', 'Emphasis on the use of nonplanar structural analysis software to analyze a buildings structural system and its individual elements.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '354', 'Numerical Analysis Laboratory', 'Lab', '2', 'An intensive survey of numerical analysis techniques used for solving engineering problems.  Topics include integration, ordinary differential equations, and the eigenproblem.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '371', 'Structural Systems Laboratory', 'Lab', '2', 'Studies in the relationship of structural framing to overall building geometry.  Emphasis on the stability of structural configurations, calculation of building loads and development of a complete gravity and lateral load path.  3 laboratories.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '3', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '412', 'Dynamics of Framed Structures', 'Lec', '1', 'Analysis of structures subjected to dynamic loads with single- and multi-degrees of freedom.  Development of techniques for analysis of structures in response to time varying loads.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '415', 'Interdisciplinary Capstone Project', 'Lab', '1', 'Team based interdisciplinary capstone / senior project course.  Analysis and evaluation of interdisciplinary challenges associated with integrating the design and construction processes to deliver a project with respect to the design, budget, schedule, quality, and performance expectations of a client.  4 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '421', 'Soil Mechanics', 'Lec', '2', 'Principles of soil mechanics, including rudiments of geology, soil classification, gravimetric and volumetric relations, compaction, methods and testing, shear strength of soil and strength theories.  2 lectures, 1 laboratory.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '421', 'Soil Mechanics', 'Lab', '4', 'Principles of soil mechanics, including rudiments of geology, soil classification, gravimetric and volumetric relations, compaction, methods and testing, shear strength of soil and strength theories.  2 lectures, 1 laboratory.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '422', 'Foundation Design', 'Lec', '2', 'Soil-bearing capacity; sizing and design of spread footings.  Design and analysis of earth-retaining structures.  Analysis of the stability of slopes.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '444', 'Reinforced Concrete Design', 'Lec', '1', 'Analytical and design principles of reinforced concrete in designing civil engineering systems.  Origin of code requirements.  Fundamentals of proportioning.  Details of elements and structural systems.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '444', 'Reinforced Concrete Design', 'Lab', '1', 'Analytical and design principles of reinforced concrete in designing civil engineering systems.  Origin of code requirements.  Fundamentals of proportioning.  Details of elements and structural systems.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '452', 'Concrete Structures Design and Constructability Laboratory', 'Lab', '4', 'Cast in place concrete framed project incorporating structural system configuration and selection, structural analysis for gravity and lateral loads, and construction drawings and specifications.  Integration of building services and architectural design, constructability issues, and relationships between construction methods and cost.  3 laboratories.  Cannot be taken concurrently with ARCE 372 or ARCE 451.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '453', 'Interdisciplinary Senior Project', 'Ind', '3', 'Interdisciplinary projects by interdisciplinary teams under faculty supervision that go beyond topics covered in the ARCE curriculum.  Projects must include integration with other disciplines outside of structural or architectural engineering.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '460', 'Collaborative Design Laboratory', 'Lab', '1', 'Investigation of the collaborative nature of the design process as it relates to the architectural engineer and related disciplines Development of skills necessary to create a successful design team through the development of specific projects.  Total credit limited to 4 units.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '476', 'Architectural Engineering Building Systems', 'Lec', '1', 'Principles and practices for the sustainable design, fabrication, and installation of architectural engineering building systems; including air/gas, water/waste water, electrical, and control systems.  Methods and materials used for fabrication and installation; including cost and schedule considerations.  3 lectures.  Not open to students with credit in ARCE/CE 475.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '485', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '495', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '503', 'Nonlinear Structural Behavior II', 'Lec', '1', 'Principles, concepts, and techniques of nonlinear structural analysis currently used in practice.  Classification of nonlinear problem types.  Investigation of typical iterative solution strategies.  Studies in material and geometric nonlinearities in spring, truss, and frame elements.  Use of current nonlinear analysis software.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '504', 'Finite Element Method for Building Structures', 'Sem', '1', 'Basic concepts of equilibrium and compatibility.  Stiffness and flexibility properties of various types of finite elements.  Development and application of displacement and force methods.  Elastic stability and dynamic response of buildings to earthquake, wind, and moving loads.  Use of finite-element computer programs.  3 seminars.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '570', 'Selected Advanced Topics', 'Lec', '1', 'Directed group study of selected topics for advanced students.  Open to undergraduate and graduate students.  The Schedule of Classes will list title selected.  Total credit limited to 8 units.  1 to 4 lectures.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', '598', 'Structural Engineering Design Project', 'Lab', '2', 'Independent development, research, and conclusion of a graduate project by individuals or teams specializing in the area of architectural or structural engineering.  Projects may include graduate students from other disciplines.  Students shall enroll in 3 quarters.  Total credit limited to 9 units.  3 laboratories.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCE', 'P353', 'Matrix Structural Computing Analysis', 'Lab', '1', 'Emphasis on the use of nonplanar structural analysis software to analyze a buildings structural system and its individual elements.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '101', 'Survey of Architectural Education and Practice', 'Lec', '1', 'Exploration of the major paradigms which have guided the development of architectural education and the profession. Survey of the roles of the architects and an introduction to curricula and programs designed to prepare students for careers in architecture. Credit/No Credit grading only. 1 lecture. Total credit limited to 3 units.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '132', 'Design and Visual Communication 1.2', 'Lab', '11', 'Continuation of ARCH 131 plus the issues, concepts, processes and skills pertaining to color theory and the design and visual communication of architectural space.  Purchase of a laptop computer, software and peripherals is highly recommended to participate in this course.  4 laboratories.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '218', 'History of World Architecture: Middle Ages - 18th Century', 'Lec', '2', 'World architecture and urbanism from the Middle Ages until the end of the 18th century Baroque. Social, cultural and physical conditions which influenced the built environment of Europe, Asia, and the Pre-Columbian and Colonial Americas. 4 lectures. Fulfills GE C3.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '242', 'Architectural Practice 2.2', 'Lec', '1', 'A continuation of ARCH 241 with an emphasis on the fundamental aspects of construction systems and the basics of construction documentation.  2 lectures, 2 activities.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '242', 'Architectural Practice 2.2', 'Act', '10', 'A continuation of ARCH 241 with an emphasis on the fundamental aspects of construction systems and the basics of construction documentation.  2 lectures, 2 activities.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '252', 'Architectural Design 2.2', 'Lab', '10', 'Continuation of ARCH 251 plus the theories, concepts, processes and skills pertaining to light, construction and function as determinants that shape the built environment and support the communication of intended concepts and meanings.  5 laboratories.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '307', 'Environmental Control Systems 2', 'Lec', '1', 'Continuation of ARCH 207 plus theory and application of climate, energy use and comfort as determinants of architectural form in large-scale buildings.  Emphasis on architectural and mechanical methods of ventilating, cooling, heating, lighting, acoustics, and water and waste systems for internal-load dominated buildings.  2 lectures, 2 discussions.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '307', 'Environmental Control Systems 2', 'Dis', '9', 'Continuation of ARCH 207 plus theory and application of climate, energy use and comfort as determinants of architectural form in large-scale buildings.  Emphasis on architectural and mechanical methods of ventilating, cooling, heating, lighting, acoustics, and water and waste systems for internal-load dominated buildings.  2 lectures, 2 discussions.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '320', 'Topics in Architectural History', 'Lec', '1', 'In-depth examination of a significant region, movement or period in architectural history, theory and criticism.  The material covered will vary depending upon the topic.  The Schedule of Classes will list topic selected.  Total credit limited to 8 units.  4 lectures.  Fulfills GE C4 except for Architecture majors.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '352', 'Architectural Design 3.2', 'Lec', '9', 'Continuation of ARCH 351 plus the development and exploration of architectural theories, building systems, and design concepts and processes involved in creating sustainable architecture with an emphasis on implications of ecological, environmental and site issues as building form generator.  1 lecture, 4 laboratories.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '352', 'Architectural Design 3.2', 'Lab', '9', 'Continuation of ARCH 351 plus the development and exploration of architectural theories, building systems, and design concepts and processes involved in creating sustainable architecture with an emphasis on implications of ecological, environmental and site issues as building form generator.  1 lecture, 4 laboratories.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '4', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '420', 'Seminar in Architectural History, Theory and Criticism', 'Sem', '1', 'Special topics based on the exploration of specific approaches, periods of time, and cultural or geographic areas.  The Schedule of Classes will list topic selected.  Total credit limited to 12 units; repeatable in same term.  4 seminars.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '443', 'Issues in Contemporary Professional Practice', 'Lec', '1', 'An exploration of topics related to the practice of architecture, including financial considerations, client obligations, project and practice management, leadership, legal responsibilities, ethics and professional judgment, and community and social responsibility, as well as the process and requirements for internship and licensure.  2 lectures, 2 discussions.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '443', 'Issues in Contemporary Professional Practice', 'Dis', '1', 'An exploration of topics related to the practice of architecture, including financial considerations, client obligations, project and practice management, leadership, legal responsibilities, ethics and professional judgment, and community and social responsibility, as well as the process and requirements for internship and licensure.  2 lectures, 2 discussions.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '452', 'Architectural Design 4.2', 'Lab', '3', 'Problems of increasing architectural complexity involving the comprehensive integration of architectural theory, design processes, and building systems with emphasis placed on multibuilding, multifunctional projects.  5 laboratories.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ARCH', '460', 'Computer Graphics Applications III', 'Lec', '1', 'Advanced methods in the application of computer graphics and multi-media techniques in architectural design.  2 lectures, 1 activity.', '3');
