INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '110', 'World of Chemistry', 'Lec', '2', 'The fundamentals of chemical cause and effect-structure/function relationships.  The basic principles of chemistry and their applications to solving human problems in organic materials science, biochemistry, toxicology, environmental science, agriculture, nutrition, and medicine.  Not open to students majoring in Chemistry or Biochemistry.  Not open to students with credit in CHEM 111, CHEM 124, or CHEM 127.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '110', 'World of Chemistry', 'Lab', '2', 'The fundamentals of chemical cause and effect-structure/function relationships.  The basic principles of chemistry and their applications to solving human problems in organic materials science, biochemistry, toxicology, environmental science, agriculture, nutrition, and medicine.  Not open to students majoring in Chemistry or Biochemistry.  Not open to students with credit in CHEM 111, CHEM 124, or CHEM 127.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '111', 'Survey of Chemistry', 'Lec', '1', 'Introduction to atomic theory, chemical reactions, bonding, stoichiometry, nomenclature, and solutions.  Intended for students who are preparing for CHEM 212/312.  Not open to students with credit in CHEM 124 or CHEM 127.  Credit will be granted in only one of the following courses:  CHEM 110, CHEM 111.  4 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '111', 'Survey of Chemistry', 'Lab', '4', 'Introduction to atomic theory, chemical reactions, bonding, stoichiometry, nomenclature, and solutions.  Intended for students who are preparing for CHEM 212/312.  Not open to students with credit in CHEM 124 or CHEM 127.  Credit will be granted in only one of the following courses:  CHEM 110, CHEM 111.  4 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '124', 'General Chemistry for Physical Science and Engineering I', 'Lec', '10', 'Stoichiometry, thermochemistry, atomic structure, bonding, solid-state structures, intermolecular forces, and foundational principles of organic chemistry.  Not open to students with credit in CHEM 127.  Credit will be granted in only one of the following courses:  CHEM 110, CHEM 111, CHEM 124.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '124', 'General Chemistry for Physical Science and Engineering I', 'Lab', '10', 'Stoichiometry, thermochemistry, atomic structure, bonding, solid-state structures, intermolecular forces, and foundational principles of organic chemistry.  Not open to students with credit in CHEM 127.  Credit will be granted in only one of the following courses:  CHEM 110, CHEM 111, CHEM 124.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '125', 'General Chemistry for Physical Science and Engineering II', 'Lec', '8', 'Topics include solution chemistry, thermodynamics, kinetics, equilibrium (including acids and bases), electrochemistry, and nuclear chemistry.  Not open to students with credit in CHEM 128.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '125', 'General Chemistry for Physical Science and Engineering II', 'Lab', '8', 'Topics include solution chemistry, thermodynamics, kinetics, equilibrium (including acids and bases), electrochemistry, and nuclear chemistry.  Not open to students with credit in CHEM 128.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '127', 'General Chemistry for Agriculture and Life Science I', 'Lec', '5', 'Introduction to atomic theory, chemical reactions, bonding, stoichiometry, nomenclature, gas laws, thermochemistry, molecular structure, and intermolecular forces.  Intended primarily for students in agriculture and life sciences.  Not open to students with credit in CHEM 124.  Credit will be granted in only one of the following courses:  CHEM 110, CHEM 111, CHEM 127.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '127', 'General Chemistry for Agriculture and Life Science I', 'Lab', '5', 'Introduction to atomic theory, chemical reactions, bonding, stoichiometry, nomenclature, gas laws, thermochemistry, molecular structure, and intermolecular forces.  Intended primarily for students in agriculture and life sciences.  Not open to students with credit in CHEM 124.  Credit will be granted in only one of the following courses:  CHEM 110, CHEM 111, CHEM 127.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '128', 'General Chemistry for Agriculture and Life Science II', 'Lec', '9', 'Continuation of CHEM 127.  Colligative properties, colloids and solutions, oxidation-reduction reactions, electrochemistry, kinetics, equilibria, and thermodynamics.  Not open to students with credit in CHEM 125.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '128', 'General Chemistry for Agriculture and Life Science II', 'Lab', '9', 'Continuation of CHEM 127.  Colligative properties, colloids and solutions, oxidation-reduction reactions, electrochemistry, kinetics, equilibria, and thermodynamics.  Not open to students with credit in CHEM 125.  3 lectures, 1 laboratory.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '200', 'Special Problems for Undergraduates', 'Ind', '2', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 4 units, with a maximum of 2 units per quarter.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '201', 'Undergraduate Research', 'Lab', '4', 'Laboratory research under faculty supervision.  Credit/No Credit grading only.  Total credit limited to 6 units.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '216', 'Organic Chemistry I', 'Lec', '2', 'Fundamental concepts and laboratory skills of organic chemistry.  Structure, bonding, nomenclature, isomerism, stereochemistry and physical properties of organic compounds.  Introduction to spectroscopy.  Reactions and mechanisms of alkanes, alkenes and alkyl halides.  Fundamental laboratory techniques in organic chemistry.  Not open to students with credit in CHEM 316.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '216', 'Organic Chemistry I', 'Lab', '6', 'Fundamental concepts and laboratory skills of organic chemistry.  Structure, bonding, nomenclature, isomerism, stereochemistry and physical properties of organic compounds.  Introduction to spectroscopy.  Reactions and mechanisms of alkanes, alkenes and alkyl halides.  Fundamental laboratory techniques in organic chemistry.  Not open to students with credit in CHEM 316.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '217', 'Organic Chemistry II', 'Lec', '5', 'Properties and reactions of carbonyl compounds, alcohols, ethers, amines and carbohydrates with an in-depth treatment of the reaction mechanisms.  Introductory concepts and applications of infrared and NMR spectroscopy.  Not open to students with credit in CHEM 317.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '220', 'Organic Chemistry Laboratory For Life Sciences II', 'Lab', '11', 'Laboratory experiments exploring reactions in organic chemistry, applying fundamental laboratory techniques covered in CHEM 216.  Not open to Chemistry and Biochemistry majors.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '221', 'Organic Chemistry Laboratory II', 'Lab', '5', 'Laboratory experiments exploring reactions in organic chemistry, applying fundamental laboratory techniques covered in CHEM 216.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '312', 'Survey of Organic Chemistry', 'Lec', '4', 'Structure, isomerism, nomenclature, fundamental reactions of major functional groups and applications of organic chemicals in agriculture, medicine, industry, and the home.  Not open to students with credit in CHEM 212 or CHEM 216/316.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '312', 'Survey of Organic Chemistry', 'Lab', '12', 'Structure, isomerism, nomenclature, fundamental reactions of major functional groups and applications of organic chemicals in agriculture, medicine, industry, and the home.  Not open to students with credit in CHEM 212 or CHEM 216/316.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '313', 'Survey of Biochemistry and Biotechnology', 'Lec', '2', 'Chemistry of biomolecules including carbohydrates, proteins, fats, vitamins, enzymes and hormones.  Basic molecular biology with applications to biotechnology and genetic engineering.  Practical intermediary metabolism of prokaryotic and eukaryotic systems.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '313', 'Survey of Biochemistry and Biotechnology', 'Lab', '6', 'Chemistry of biomolecules including carbohydrates, proteins, fats, vitamins, enzymes and hormones.  Basic molecular biology with applications to biotechnology and genetic engineering.  Practical intermediary metabolism of prokaryotic and eukaryotic systems.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '349', 'Chemical and Biological Warfare', 'Sem', '1', 'History, development, and use of chemical and biological warfare (CBW).  Chemical and biological disarmament.  Production and destruction of CBW agents.  Uses of CBW.  CBW terrorism.  Ethics of CBW.  2 lectures, 2 seminars.  Fulfills GE Area F.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '349', 'Chemical and Biological Warfare', 'Lec', '1', 'History, development, and use of chemical and biological warfare (CBW).  Chemical and biological disarmament.  Production and destruction of CBW agents.  Uses of CBW.  CBW terrorism.  Ethics of CBW.  2 lectures, 2 seminars.  Fulfills GE Area F.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '351', 'Physical Chemistry I', 'Lec', '1', 'Basic physical chemistry for the study of chemical and biochemical systems.  Kinetic-molecular theory, gas laws, principles of thermodynamics.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '352', 'Physical Chemistry II', 'Lec', '1', 'Application of physical chemistry to chemical and biochemical systems.  Electrochemistry, kinetics, viscosity, surface and transport properties.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '354', 'Physical Chemistry Laboratory', 'Lab', '2', 'Experimental studies of gases, solutions, thermochemistry, chemical and phase equilibria, electrochemistry, chemical and enzyme kinetics, computational methods and applications to chemistry and biochemistry.  Applicable literature and databases.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '371', 'Biochemical Principles', 'Lec', '1', 'Chemistry and function of major cellular constituents:  proteins, lipids, carbohydrates, and membranes.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '371', 'Biochemical Principles', 'Lab', '3', 'Chemistry and function of major cellular constituents:  proteins, lipids, carbohydrates, and membranes.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '373', 'Molecular Biology', 'Lec', '1', 'Structure of nucleic acids and chromosomes.  Mechanisms and regulation of nucleic acid and protein synthesis.  Molecular biology techniques.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '377', 'Chemistry of Drugs and Poisons', 'Lec', '1', 'Introduction to pharmacology and toxicology:  history, sources, development and testing, physical and chemical properties, biochemical and physiological effects, mechanisms of action, and the therapeutic uses and toxicology of common drugs and poisons.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '400', 'Special Problems for Advanced Undergraduates', 'Lab', '4', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '401', 'Advanced Undergraduate Research', 'Lab', '9', 'Laboratory research under faculty supervision.  Credit/No Credit grading only.  Total credit limited to 6 units.  4 units may be applied to approved chemistry electives.  1-3 laboratories.', '1-3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '414', 'Advanced Organic Chemistry - Mechanisms', 'Lec', '1', 'A mechanistic study of organic reactions; correlation of structure with reactivity; reaction intermediates and species involved in reactions; methods of probing reaction mechanisms.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '439', 'Instrumental Analysis', 'Lec', '1', 'Theory, practice and method selection of modern instrumental analytical techniques, including spectroscopic, electrochemical, chromatographic and thermal methods.  Current industrial applications.  Laboratory work emphasizes optimization of experimental parameters.  3 lectures, 2 laboratories.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '439', 'Instrumental Analysis', 'Lab', '2', 'Theory, practice and method selection of modern instrumental analytical techniques, including spectroscopic, electrochemical, chromatographic and thermal methods.  Current industrial applications.  Laboratory work emphasizes optimization of experimental parameters.  3 lectures, 2 laboratories.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '445', 'Polymers & Coatings II', 'Lec', '1', 'Introduction to polymerization methods and mechanisms.  Chemistry of initiators, catalysts and inhibitors, kinetics of polymerization.  Uses of representative polymer types.  Synthesis, film formation, structure and properties of polymers commonly used in coatings and adhesives.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '448', 'Polymers and Coatings Laboratory II', 'Lab', '1', 'Polymer synthesis using solution, suspension, bulk, emulsion techniques.  Synthesis of chain growth polymers using free radical, anionic, cationic, and other catalysts.  Synthesis of step-growth polymers.  Kinetics of polymer reactions.  Synthesis of resins used in modern coatings.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '449', 'Polymers and Coatings Internship', 'Ind', '1', 'Selected students will spend up to 12 weeks with an approved polymers and coatings firm engaged in production or related business.  Time will be spent applying and developing production and technical skills and abilities in the polymers and coatings industry.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '459', 'Undergraduate Seminar', 'Sem', '1', 'Review of current research, experiments, and problems related to the students major field of interest.  Preparation and presentation of reports on problems or research activities.  2 seminars.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '461', 'Senior Project Report', 'Ind', '2', 'A comprehensive synthesis of professional literature that integrates content from kinesiology courses resulting in a report.  Topic must be approved by the instructor.  Minimum 30 hours.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '463', 'Honors Research', 'Lab', '1', 'Advanced laboratory research.  Results are presented in a poster session or other public forum.  Total credit limited to 2 units with a maximum of 1 unit per quarter.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '465', 'College Teaching Practicum', 'Ind', '2', 'Teaching assignment in an undergraduate college classroom.  Includes teaching and related activities under the direction of a permanent faculty member in the Department of Chemistry and Biochemistry.  Total credit limited to 4 units.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '466', 'Learning Assistant Seminar', 'Sem', '1', 'Pedagogical instruction and introduction to education research for Chemistry Learning Assistants.  Effective questioning, the effect of explanatory knowledge on student learning, student misconceptions in chemistry, collaborative problem solving techniques in chemistry, studio curriculum development, content in the general chemistry curriculum.  Total credit limited to 4 units.  Credit/No Credit grading only.  2 seminars.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '474', 'Protein Techniques Laboratory', 'Lec', '1', 'Experiments in protein purification and analysis from recombinant sources.  Ion-exchange and affinity chromatography, electrophoresis and blotting.  UV, chemical, immune, and fluorescent detection.  Enzyme kinetic analysis.  1 lecture, 2 laboratories.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '474', 'Protein Techniques Laboratory', 'Lab', '2', 'Experiments in protein purification and analysis from recombinant sources.  Ion-exchange and affinity chromatography, electrophoresis and blotting.  UV, chemical, immune, and fluorescent detection.  Enzyme kinetic analysis.  1 lecture, 2 laboratories.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '485', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '495', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '500', 'Special Problems for Graduate Students', 'Ind', '1', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 8 units.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '528', 'Nutritional Biochemistry', 'Sem', '1', 'Nutritional aspects of biochemistry.  Lecture, library research and student presentations.  Topics include vitamins and minerals, essential and energy providing nutrients, deficiency, degenerative and genetic diseases of metabolism.  Emphasis on current research and controversy.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '545', 'Polymer Synthesis and Mechanisms', 'Lec', '1', 'Polymerization methods and mechanisms; chemistry of initiators, catalysts and inhibitors; use of representative types; synthesis, structure and properties of polymers commonly used in coatings and adhesives.  Special individual project.  Not open to students with credit in CHEM 445.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '548', 'Polymer Synthesis Laboratory', 'Lab', '1', 'Polymer synthesis using solution, suspension, bulk, emulsion techniques.  Synthesis of chain growth polymers using free radical, anionic, cationic, and other catalysts.  Synthesis of step-growth polymers.  Kinetics of polymer reactions.  Synthesis of resins used in modern coatings.  Special individual project.  Not open to students with credit in CHEM 448.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '590', 'Graduate Seminar in Polymers and Coatings', 'Sem', '1', 'Problems and topics in polymers and coatings selected according to the interest and needs of the students enrolled.  Total credit limited to 3 units.  1 seminar.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '598', 'Graduate Project', 'Ind', '1', 'Supervised industrial graduate internship in polymers and coatings science.  Provides students with industrial research experience.  Requires approval of graduate advisor.  Students engage in industrial research and development at an approved industry, make regular reports back to graduate advisor, and present formal report and seminar on work each quarter.  Total credit limited to 9 units.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', '599', 'Graduate Thesis', 'Ind', '1', 'Directed graduate research in specialized advanced topics related to polymers and coatings science, leading to a graduate thesis of suitable quality.  Requires approval of graduate advisor.  Students are expected to work independently and report weekly to faculty advisor.  Total credit limited to 9 units.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', 'P124', 'General Chemistry for Physical Science and Engineering I', 'Lec', '1', 'Stoichiometry, thermochemistry, atomic structure, bonding, solid-state structures, intermolecular forces, and foundational principles of organic chemistry.  Not open to students with credit in CHEM 127.  Credit will be granted in only one of the following courses:  CHEM 110, CHEM 111, CHEM 124.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', 'P124', 'General Chemistry for Physical Science and Engineering I', 'Lab', '1', 'Stoichiometry, thermochemistry, atomic structure, bonding, solid-state structures, intermolecular forces, and foundational principles of organic chemistry.  Not open to students with credit in CHEM 127.  Credit will be granted in only one of the following courses:  CHEM 110, CHEM 111, CHEM 124.  3 lectures, 1 laboratory.  Fulfills GE B3 & B4.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', 'P217', 'Organic Chemistry II', 'Lec', '1', 'Properties and reactions of carbonyl compounds, alcohols, ethers, amines and carbohydrates with an in-depth treatment of the reaction mechanisms.  Introductory concepts and applications of infrared and NMR spectroscopy.  Not open to students with credit in CHEM 317.  3 lectures.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', 'P220', 'Organic Chemistry Laboratory For Life Sciences II', 'Lab', '1', 'Laboratory experiments exploring reactions in organic chemistry, applying fundamental laboratory techniques covered in CHEM 216.  Not open to Chemistry and Biochemistry majors.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', 'P313', 'Survey of Biochemistry and Biotechnology', 'Lec', '1', 'Chemistry of biomolecules including carbohydrates, proteins, fats, vitamins, enzymes and hormones.  Basic molecular biology with applications to biotechnology and genetic engineering.  Practical intermediary metabolism of prokaryotic and eukaryotic systems.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHEM', 'P313', 'Survey of Biochemistry and Biotechnology', 'Lab', '1', 'Chemistry of biomolecules including carbohydrates, proteins, fats, vitamins, enzymes and hormones.  Basic molecular biology with applications to biotechnology and genetic engineering.  Practical intermediary metabolism of prokaryotic and eukaryotic systems.  4 lectures, 1 laboratory.', '5');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHIN', '102', 'Elementary Mandarin Chinese II', 'Lec', '2', 'Class practice and assigned outside work in pronunciation, sentence structure, reading, writing and introduction to Chinese culture.  To be taken in numerical sequence.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHIN', '102', 'Elementary Mandarin Chinese II', 'Act', '2', 'Class practice and assigned outside work in pronunciation, sentence structure, reading, writing and introduction to Chinese culture.  To be taken in numerical sequence.  3 lectures, 1 activity.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHIN', '202', 'Intermediate Mandarin Chinese II', 'Lec', '1', 'Development of presentational, interpersonal, and interpretive intermediate-level language skills in Mandarin Chinese within an intercultural context.  To be taken in numerical sequence.  3 lectures, 1 activity.  Fulfills GE C5.  Formerly CHIN 122.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('CHIN', '202', 'Intermediate Mandarin Chinese II', 'Act', '1', 'Development of presentational, interpersonal, and interpretive intermediate-level language skills in Mandarin Chinese within an intercultural context.  To be taken in numerical sequence.  3 lectures, 1 activity.  Fulfills GE C5.  Formerly CHIN 122.', '4');
