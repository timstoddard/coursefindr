INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '120', 'Principles of Horticulture and Crop Science', 'Lec', '1', 'Introduction to horticulture and crop science. Basic plant processes, classification, anatomy, physiology, and biotechnology. Effect of environment on plants and how we control it. Introduction to plant growth including propagation, media, irrigation, nutrition, management, harvest, and post harvest handling. Peoples use of plants. Field trip required. 3 lectures, 1 laboratory. Formerly HCS 120.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '120', 'Principles of Horticulture and Crop Science', 'Lab', '3', 'Introduction to horticulture and crop science. Basic plant processes, classification, anatomy, physiology, and biotechnology. Effect of environment on plants and how we control it. Introduction to plant growth including propagation, media, irrigation, nutrition, management, harvest, and post harvest handling. Peoples use of plants. Field trip required. 3 lectures, 1 laboratory. Formerly HCS 120.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '123', 'Landscape Installation and Maintenance', 'Lec', '1', 'Planting and maintenance of trees, shrubs, ground covers, perennial plantings, color beds, specialty plantings, and small turf areas.  Site selection, cultural requirements, scheduling of maintenance activities, pruning, landscape renovation and irrigation system repair.  Equipment operation, maintenance, and safety.  Speakers from industry.  3 lectures, 1 laboratory.  Formerly EHS 123.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '123', 'Landscape Installation and Maintenance', 'Lab', '1', 'Planting and maintenance of trees, shrubs, ground covers, perennial plantings, color beds, specialty plantings, and small turf areas.  Site selection, cultural requirements, scheduling of maintenance activities, pruning, landscape renovation and irrigation system repair.  Equipment operation, maintenance, and safety.  Speakers from industry.  3 lectures, 1 laboratory.  Formerly EHS 123.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '127', 'Horticulture and Landscape Design', 'Lec', '1', 'Aesthetic aspects of environmental horticulture, introduction to computer aided design, presentation techniques and garden history. Field trip required. 2 lectures, 2 laboratories. Formerly EHS 127.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '127', 'Horticulture and Landscape Design', 'Lab', '1', 'Aesthetic aspects of environmental horticulture, introduction to computer aided design, presentation techniques and garden history. Field trip required. 2 lectures, 2 laboratories. Formerly EHS 127.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '132', 'Pomology I', 'Lec', '1', 'Orchard design and development, cultural practices, physiological responses of trees to cultural practices, propagation and strategies to maximize orchard profitability and sustainability.  Not open to students with credit in AEPS 250.  3 lectures, 1 laboratory.  Formerly FRSC 132.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '132', 'Pomology I', 'Lab', '2', 'Orchard design and development, cultural practices, physiological responses of trees to cultural practices, propagation and strategies to maximize orchard profitability and sustainability.  Not open to students with credit in AEPS 250.  3 lectures, 1 laboratory.  Formerly FRSC 132.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '200', 'Special Problems for Undergraduates', 'Ind', '11', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 4 units, with a maximum of 2 units per quarter.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '202', 'Fruit Enterprise Project', 'Lec', '1', 'Beginning field experience in management of orchards or honeybees. May include cultural practices, harvesting, processing, sales and marketing activities. Credit/No Credit grading only. 1 lecture, 1 activity. Formerly FRSC 202.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '202', 'Fruit Enterprise Project', 'Act', '1', 'Beginning field experience in management of orchards or honeybees. May include cultural practices, harvesting, processing, sales and marketing activities. Credit/No Credit grading only. 1 lecture, 1 activity. Formerly FRSC 202.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '203', 'Organic Enterprise', 'Lec', '1', 'Beginning field experience in production and marketing of organic vegetable crops. May include cultural practices, harvesting, processing, sales and marketing activities. Credit/No Credit grading only. 1 lecture, 1 activity. Formerly CRSC 203.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '203', 'Organic Enterprise', 'Act', '1', 'Beginning field experience in production and marketing of organic vegetable crops. May include cultural practices, harvesting, processing, sales and marketing activities. Credit/No Credit grading only. 1 lecture, 1 activity. Formerly CRSC 203.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '204', 'Vegetable Enterprise Project', 'Lec', '3', 'Beginning field experience in vegetable production systems. May include cultural practices, harvesting, processing, sales and marketing activities. Credit/No Credit grading only. 1 lecture, 1 activity. Formerly VGSC 202.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '204', 'Vegetable Enterprise Project', 'Act', '3', 'Beginning field experience in vegetable production systems. May include cultural practices, harvesting, processing, sales and marketing activities. Credit/No Credit grading only. 1 lecture, 1 activity. Formerly VGSC 202.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '212', 'Environmental Horticulture Enterprise Project I', 'Ind', '3', 'Selection and completion of a management/production project under faculty supervision.  Project participation is voluntary and subject to approval by the department head and the Cal Poly Corporation.  Degree credit limited to two units.  Credit/No Credit grading only.  Formerly EHS 210.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '215', 'Floral Design I', 'Lec', '1', 'Fundamentals of theory, techniques and skills currently practiced in the floral industry. Intended as consumer education for non-majors as well as initial preparation for pre-professionals. Includes applied art principles, post-harvest care and handling practices, and proper use of florist tools and materials in developing basic designs. 1 lecture, 2 laboratories. Formerly EHS 215.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '215', 'Floral Design I', 'Lab', '1', 'Fundamentals of theory, techniques and skills currently practiced in the floral industry. Intended as consumer education for non-majors as well as initial preparation for pre-professionals. Includes applied art principles, post-harvest care and handling practices, and proper use of florist tools and materials in developing basic designs. 1 lecture, 2 laboratories. Formerly EHS 215.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '244', 'Precision Farming', 'Lec', '1', 'Precision agriculture applications.  Integrating GIS, GPS, and remote sensing technologies with site-specific farming practices to optimize agricultural productivity.  Field trip required.  3 lectures, 1 laboratory.  Crosslisted as AEPS/BRAE 244.  Formerly CRSC 244.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '244', 'Precision Farming', 'Lab', '1', 'Precision agriculture applications.  Integrating GIS, GPS, and remote sensing technologies with site-specific farming practices to optimize agricultural productivity.  Field trip required.  3 lectures, 1 laboratory.  Crosslisted as AEPS/BRAE 244.  Formerly CRSC 244.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '304', 'Plant Breeding', 'Lec', '1', 'Principles and techniques used to develop new plant varieties.  Sexual reproduction, inheritance, selection and biotechnology methods useful in breeding of plants.  Field trip required.  3 lectures, 1 laboratory.  Formerly HCS 304.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '304', 'Plant Breeding', 'Lab', '2', 'Principles and techniques used to develop new plant varieties.  Sexual reproduction, inheritance, selection and biotechnology methods useful in breeding of plants.  Field trip required.  3 lectures, 1 laboratory.  Formerly HCS 304.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '312', 'Environmental Horticulture Enterprise Project II', 'Ind', '3', 'Selection and completion of a management/production project under faculty supervision.  Project participation is voluntary and subject to approval by the department head and the Cal Poly Corporation.  Degree credit limited to 2 units.  Maximum degree credit for AEPS 212 and AEPS 312 limited to 4 units.  Credit/No Credit grading only.  Formerly EHS 310.', '2-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '327', 'Vertebrate Pest Management', 'Lec', '1', 'Vertebrate pests injurious to crops, livestock, forest products, wildlife, stored products and humans.  Life habits, identification, control methods, and materials.  Related laws and regulations.  3 lectures, 1 laboratory.  Formerly PPSC 327.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '327', 'Vertebrate Pest Management', 'Lab', '2', 'Vertebrate pests injurious to crops, livestock, forest products, wildlife, stored products and humans.  Life habits, identification, control methods, and materials.  Related laws and regulations.  3 lectures, 1 laboratory.  Formerly PPSC 327.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '339', 'Internship in Horticulture and Crop Science', 'Ind', '1', 'Selected Horticulture and Crop Science students will spend up to 12 weeks with an approved agricultural/horticultural firm engaged in production or related business.  Time will be spent applying and developing production and managerial skills and abilities.  One unit of credit may be allowed for each full week of completed and reported internship.  Degree credit limited to 6 units.  Credit/No Credit grading only.  Formerly HCS 339.', '1-12');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '342', 'Potted Plant Production', 'Lec', '1', 'Production of major commercial flowering potted plants in greenhouses and outdoors.  Preparation and scheduling of potted flowering greenhouse crops for major markets.  Field trip required.  3 lectures, 1 laboratory.  Formerly EHS 342.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '342', 'Potted Plant Production', 'Lab', '1', 'Production of major commercial flowering potted plants in greenhouses and outdoors.  Preparation and scheduling of potted flowering greenhouse crops for major markets.  Field trip required.  3 lectures, 1 laboratory.  Formerly EHS 342.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '350', 'Abiotic Plant Problems', 'Lec', '1', 'Diagnosis of physiological disorders associated with environmental and nutritional factors.  Particular emphasis on the systematic inquiry process.  Case histories, multimedia use.  2 lectures, 1 laboratory.  Formerly HCS 327.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '350', 'Abiotic Plant Problems', 'Lab', '1', 'Diagnosis of physiological disorders associated with environmental and nutritional factors.  Particular emphasis on the systematic inquiry process.  Case histories, multimedia use.  2 lectures, 1 laboratory.  Formerly HCS 327.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '351', 'Experimental Techniques and Analysis', 'Lec', '1', 'Principal experimental designs used in agriculture and methods of statistical analysis of data collected from each.  Statistical software.  Field practice in planning and layout of typical experiments.  3 lectures, 1 laboratory.  Formerly CRSC 411.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '351', 'Experimental Techniques and Analysis', 'Lab', '2', 'Principal experimental designs used in agriculture and methods of statistical analysis of data collected from each.  Statistical software.  Field practice in planning and layout of typical experiments.  3 lectures, 1 laboratory.  Formerly CRSC 411.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '381', 'Native Plants for California Landscapes', 'Lec', '1', 'Horticultural investigation of the California flora with emphasis on landscape use and potential.  Plant recognition, identification, propagation and culture.  Utilization of native plants in landscape design and habitat restoration.  Field trip required.  3 lectures, 1 laboratory.  Formerly EHS 381.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '381', 'Native Plants for California Landscapes', 'Lab', '1', 'Horticultural investigation of the California flora with emphasis on landscape use and potential.  Plant recognition, identification, propagation and culture.  Utilization of native plants in landscape design and habitat restoration.  Field trip required.  3 lectures, 1 laboratory.  Formerly EHS 381.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '12', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '402', 'Fruit Enterprise Project Management', 'Lec', '1', 'Advanced experience in production of orchards.  Development and execution of a plan for field operations, fruit processing and/or marketing.  Management decision-making.  Total credit limited to 4 units.  Credit/No Credit grading only.  1 lecture, 1 activity.  Formerly FRSC 402.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '402', 'Fruit Enterprise Project Management', 'Act', '1', 'Advanced experience in production of orchards.  Development and execution of a plan for field operations, fruit processing and/or marketing.  Management decision-making.  Total credit limited to 4 units.  Credit/No Credit grading only.  1 lecture, 1 activity.  Formerly FRSC 402.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '403', 'Organic Enterprise Project Management', 'Lec', '1', 'Advanced experience in production of organic vegetables.  Development and execution of a plan for planting schedule, cultivation, harvest, and/or marketing.  Management decision-making.  Total credit limited to 4 units.  Credit/No Credit grading only.  1 lecture, 1 activity.  Formerly CRSC 402.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '403', 'Organic Enterprise Project Management', 'Act', '1', 'Advanced experience in production of organic vegetables.  Development and execution of a plan for planting schedule, cultivation, harvest, and/or marketing.  Management decision-making.  Total credit limited to 4 units.  Credit/No Credit grading only.  1 lecture, 1 activity.  Formerly CRSC 402.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '404', 'Vegetable Enterprise Project Management', 'Lec', '2', 'Advanced experience in the production of vegetable crops.  Development, management and implementation of cultural practices, harvesting, processing, sales and marketing activities for vegetable crops.  Total credit limited to 4 units.  Credit/No Credit grading only.  1 lecture, 1 activity.  Formerly VGSC 402.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '404', 'Vegetable Enterprise Project Management', 'Act', '2', 'Advanced experience in the production of vegetable crops.  Development, management and implementation of cultural practices, harvesting, processing, sales and marketing activities for vegetable crops.  Total credit limited to 4 units.  Credit/No Credit grading only.  1 lecture, 1 activity.  Formerly VGSC 402.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '406', 'Advanced Weed Management', 'Lec', '1', 'Planning, design and implementation of long-term sustainable weed management programs.  Analysis of traditional and new technologies for weed management based on their impact on agriculture, society and the environment.  3 lectures, 1 laboratory.  Formerly PPSC 405.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '406', 'Advanced Weed Management', 'Lab', '1', 'Planning, design and implementation of long-term sustainable weed management programs.  Analysis of traditional and new technologies for weed management based on their impact on agriculture, society and the environment.  3 lectures, 1 laboratory.  Formerly PPSC 405.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '421', 'Postharvest Technology of Horticultural Crops', 'Lec', '1', 'Respiration, ethylene, ripening and senescence; modified atmosphere packaging, controlled atmosphere storage, packinghouses and transportation; survey of postharvest techniques to maximize commodity shelf-life.  Field trip required.  3 lectures, 1 laboratory.  Formerly HCS 421.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '421', 'Postharvest Technology of Horticultural Crops', 'Lab', '2', 'Respiration, ethylene, ripening and senescence; modified atmosphere packaging, controlled atmosphere storage, packinghouses and transportation; survey of postharvest techniques to maximize commodity shelf-life.  Field trip required.  3 lectures, 1 laboratory.  Formerly HCS 421.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '424', 'Nursery Crop Production', 'Lec', '1', 'Comprehensive and historical overview of the nursery industry.  Types of wholesale nurseries and their products.  Plant production systems, scheduling, and marketing.  Emphasis on medium to large woody plants and deciduous field-grown ornamental trees and shrubs in the western U.S.  Field trip required.  3 lectures, 1 laboratory.  Formerly EHS 424.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '424', 'Nursery Crop Production', 'Lab', '1', 'Comprehensive and historical overview of the nursery industry.  Types of wholesale nurseries and their products.  Plant production systems, scheduling, and marketing.  Emphasis on medium to large woody plants and deciduous field-grown ornamental trees and shrubs in the western U.S.  Field trip required.  3 lectures, 1 laboratory.  Formerly EHS 424.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '427', 'Disease and Pest Control Systems for Ornamental Plants', 'Lec', '1', 'Recognition, prevention and control of diseases, insect/mite pests and weeds that impact commercial ornamental plantings.  Integrated pest management strategies presented including biological, cultural, and safe and proper pesticidal controls.  Laboratory emphasizes monitoring, problem solving and application of appropriate pest control measures.  3 lectures, 1 laboratory.  Formerly PPSC 427.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '427', 'Disease and Pest Control Systems for Ornamental Plants', 'Lab', '1', 'Recognition, prevention and control of diseases, insect/mite pests and weeds that impact commercial ornamental plantings.  Integrated pest management strategies presented including biological, cultural, and safe and proper pesticidal controls.  Laboratory emphasizes monitoring, problem solving and application of appropriate pest control measures.  3 lectures, 1 laboratory.  Formerly PPSC 427.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '431', 'Insect Pest Management', 'Lec', '1', 'Principles of insect and mite pest management, including integrated pest management (IPM), applications of ecological theory to pest management, cultural, biological and chemical controls, pesticide resistance management, insect and mite monitoring, biotechnology applications, pesticide laws and regulations, pest control advisor and qualified applicator licensing and certification.  Field trip required.  3 lectures, 1 laboratory.  Formerly PPSC 431.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '431', 'Insect Pest Management', 'Lab', '1', 'Principles of insect and mite pest management, including integrated pest management (IPM), applications of ecological theory to pest management, cultural, biological and chemical controls, pesticide resistance management, insect and mite monitoring, biotechnology applications, pesticide laws and regulations, pest control advisor and qualified applicator licensing and certification.  Field trip required.  3 lectures, 1 laboratory.  Formerly PPSC 431.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '445', 'Cropping Systems', 'Lec', '1', 'Classification and description of agricultural systems of the world.  Cropping systems as land management plans.  Systems approaches to improvement of agricultural situations.  Consideration of human factors and the agroecosystem in efforts to create a more sustainable agriculture.  Field trip required.  3 lectures, 1 activity.  Formerly CRSC 445.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '445', 'Cropping Systems', 'Act', '1', 'Classification and description of agricultural systems of the world.  Cropping systems as land management plans.  Systems approaches to improvement of agricultural situations.  Consideration of human factors and the agroecosystem in efforts to create a more sustainable agriculture.  Field trip required.  3 lectures, 1 activity.  Formerly CRSC 445.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '461', 'Senior Project I', 'Ind', '1', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '462', 'Senior Project II', 'Ind', '11', 'Selection and completion of a project under faculty supervision. Projects typical of problems which graduates must solve in their fields of employment. Project results are presented in a formal report. Minimum 90 hours total time.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '500', 'Individual Study in Horticulture and Crop Science', 'Ind', '10', 'Advanced independent study planned and completed under the direction of a member of the Horticulture and Crop Science faculty.  Total credit limited to 6 units.  Formerly HCS 500.', '1-6');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '539', 'Graduate Internship in Horticulture and Crop Science', 'Ind', '1', 'Application of theory to the solution of problems of agricultural production or related business in the fields of horticulture and crop science.  Analyze specific management problems and perform general management assignments detailed in a contract between the student, the firm or organization, and the faculty advisor before the internship commences.  Degree credit limited to 6 units.  Formerly HCS 539.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '596', 'Thesis in Crop Science', 'Ind', '3', 'Systematic research of a significant problem in Crop Science.  Thesis will include problem identification, significance, methods, data analysis, and conclusion.  Students must enroll every quarter in which facilities are used or advisement is received.  Degree credit limited to 6 units.  Formerly CRSC 599.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '597', 'Thesis in Environmental Horticulture Science', 'Ind', '2', 'Systematic research of a significant problem in environmental horticulture.  Thesis will include problem identification, significance, methods, data analysis and conclusion.  Students must enroll every quarter in which facilities are used or advisement is received.  Degree credit limited to 6 units.  Formerly EHS 599.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '598', 'Thesis in Fruit Science', 'Ind', '1', 'Systematic research of a significant problem in Fruit Science.  Thesis will include problem identification, significance, methods, data analysis, and conclusion.  Students must enroll every quarter in which facilities are used or advisement is received.  Degree credit limited to 6 units.  Formerly FRSC 599.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AEPS', '599', 'Thesis in Plant Protection Science', 'Ind', '3', 'Systematic research of a topic in plant protection science, including weed science, entomology, plant pathology, nematology or vertebrate management.  Thesis to describe the problem and its significance, methodology, results, data analysis, discussion and conclusion.  Enrollment required every quarter in which facilities are used or advisement received.  Degree credit limited to 6 units.  Total credit limited to 9 units.  Formerly PPSC 599.', '1-9');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '200', 'Special Problems for Undergraduates', 'Ind', '9', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 4 units, with a maximum of 2 units per quarter.', '1-2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '215', 'Introduction to Aerospace Design', 'Lab', '2', 'Introduction to problem solving techniques and team-centered design projects in aerospace engineering.  Primary emphasis on the solutions of design problems in aerospace engineering using computers.  2 laboratories.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '303', 'Aerothermodynamics III', 'Lec', '2', 'Properties and characteristics of fluids, fluid statics and dynamics, the thermodynamic relations, laminar and turbulent flows, subsonic and supersonic flows as applied to flight vehicles.  Introduction to heat transfer.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '304', 'Experimental Aerothermodynamics', 'Lec', '2', 'Laboratory experiments verify the momentum and energy equations.  Mass flow rate, fan performance, boundary layer measurements, diffuser performance, and induction pump performance experiments are evaluated.  Introduction to electronic sensors, signals and data acquisition.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '304', 'Experimental Aerothermodynamics', 'Lab', '6', 'Laboratory experiments verify the momentum and energy equations.  Mass flow rate, fan performance, boundary layer measurements, diffuser performance, and induction pump performance experiments are evaluated.  Introduction to electronic sensors, signals and data acquisition.  1 lecture, 1 laboratory.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '310', 'Air and Space', 'Lec', '3', 'Technological innovations that have led to modern aircraft and spacecraft as viewed from an historical perspective.  Development of aerodynamics, propulsion systems, light-weight structures, and control systems.  How aviation has affected, and been affected by, history.  Impact of aviation on society, including civil and military aircraft/spacecraft.  Federal regulation of aviation, including air traffic control and airlines.  Future developments in air and space technology.  4 lectures.  Crosslisted as AERO/HNRS 310.  Fulfills GE Area F.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '331', 'Aerospace Structural Analysis I', 'Lec', '2', 'Deflection analysis.  Principles of fictitious displacement, virtual work, and unit load method.  Energy methods:  Castiglianos theorem, Maxwell-Betti reciprocal theorem, minimum principles, Rayleigh-Ritzs method and Galerkins method.  Stress analysis of aircraft and spacecraft components.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '353', 'Spacecraft Environment', 'Lec', '1', 'Effects of the space environment on a spacecraft and design considerations.  Topics include the launch, vacuum, particulate, plasma, and radiation environments 4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '400', 'Special Problems for Advanced Undergraduates', 'Ind', '9', 'Individual investigation, research, studies, or surveys of selected problems.  Total credit limited to 6 units, with a maximum of 4 units per quarter.', '1-4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '420', 'Aircraft Dynamics and Control', 'Lec', '1', 'Newtons 6-degree-of-freedom equations of motion applied to aerospace vehicles.  Stability and control derivatives, reference frames, steady-state and perturbed dynamic analyses applied to aerospace vehicles.  Stability and control design principles applied to transfer functions, state-space, and modal system dynamics.  4 lectures.', '4');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '433', 'Experimental Stress Analysis', 'Lab', '2', 'Employing the knowledge of stress analysis and aerospace structural analysis in an individual and group design project dealing with aerospace structures.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '444', 'Aircraft Design II', 'Lab', '2', 'Preliminary layout of a typical aircraft vehicle using design and calculation techniques developed in previous aerospace engineering courses.  Design of a flight vehicle, including its structures and systems.  Preparation of necessary drawings and a report.  3 laboratories.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '448', 'Spacecraft Design II', 'Lab', '2', 'Preliminary layout of typical space vehicle using design and calculation techniques developed in previous aerospace engineering courses.  Design of selected components and preparation of necessary drawings.  3 laboratories.  Open to students enrolled in the multidisciplinary design minor.', '3');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '465', 'Aerospace Systems Senior Laboratory', 'Lab', '2', 'Culminating laboratory based experience.  Experiments require the integration of the many disciplines in Aerospace Engineering.  The successful completion of each experiment requires synthesis and integration of the fundamental concepts of the engineering sciences.  Experimentation in the areas of aeroelasticity, active vibration control, inertial navigation, thermal control, hardware-in-the-loop simulation, and momentum exchange.  1 laboratory.', '1');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AERO', '493', 'Cooperative Education Experience', 'Ind', '1', 'Full-time work experience in business, industry, government, and other areas of student career interest.  Positions are paid and usually require relocation and registration in course for two consecutive quarters.  Formal report and evaluation by work supervisor required.  Major credit limited to 12 units; total credit limited to 24 units.  Credit/No Credit grading only.', '12');
