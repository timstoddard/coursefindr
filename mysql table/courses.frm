INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('DANC', '346', 'Dance Production', 'Lab', '1', 'Directed production of annual Orchesis Dance Company Concert and other public performances.  Attendance of professional dance concert(s) required.  Purchase of concert ticket(s) may be required.  Total credit limited to 16 units.  1 activity, 3 laboratories.', '4');
