INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGR', '322', 'The Learn By Doing Lab Teaching Practicum', 'Sem', '1', 'Early teaching experience in an informal science/technology/engineering/ mathematics (STEM) teaching and learning environment.  Principles of inquiry-driven STEM education, lesson design, implementation and assessment.  Intended for undergraduates exploring STEM teaching as a career.  Total credit limited to 4 units.  Credit/No Credit grading only.  1 seminar, 1 laboratory.  Crosslisted as ENGR 322/SCM 302/HNRS 302.', '2');
INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('ENGR', '322', 'The Learn By Doing Lab Teaching Practicum', 'Lab', '4', 'Early teaching experience in an informal science/technology/engineering/ mathematics (STEM) teaching and learning environment.  Principles of inquiry-driven STEM education, lesson design, implementation and assessment.  Intended for undergraduates exploring STEM teaching as a career.  Total credit limited to 4 units.  Credit/No Credit grading only.  1 seminar, 1 laboratory.  Crosslisted as ENGR 322/SCM 302/HNRS 302.', '2');
