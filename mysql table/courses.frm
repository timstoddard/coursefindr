INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('AG', '452', 'Issues Affecting California Agriculture', 'Sem', '1', 'Interactive seminars with speakers from government and industry covering policy and regulations affecting California agriculture.  Students develop an understanding of agricultural policy and work in teams to develop a public presentation and position paper on a significant issue.  Field trip to Sacramento required.  4 seminars.', '4');
