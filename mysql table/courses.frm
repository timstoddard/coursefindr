INSERT INTO courses(dept, courseNumber, name, type, sections, description, units) VALUES ('SPAN', '111', 'Elementary Hispanic Language and Culture', 'Lec', '1', 'Beginning Spanish. Class practice and assigned outside work in pronunciation, sentence structure, reading, writing, and conversation with special focus on vocabulary and culture from American agribusiness and the Hispanic cultures of the United States and Latin America. Not open to students who have credit in SPAN 101. 3 lectures, 1 activity. Fulfills USCP.', '4');
